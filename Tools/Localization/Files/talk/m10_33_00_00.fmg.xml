<text id="75601747">%null%</text>
<text id="75601748">%null%</text>
<text id="75601749">%null%</text>
<text id="75601750">My servant, human scent lingers upon thee yet.</text>
<text id="75601751">%null%</text>
<text id="75601752">%null%</text>
<text id="75601753">%null%</text>
<text id="75601754">%null%</text>
<text id="75601755">%null%</text>
<text id="75601756">%null%</text>
<text id="75601757">%null%</text>
<text id="75601758">%null%</text>
<text id="75601759">%null%</text>
<text id="75601760">Rid thyself of it, if thou canst.</text>
<text id="75601800">My servant, well met indeed.</text>
<text id="75601801">%null%</text>
<text id="75601802">%null%</text>
<text id="75601803">%null%</text>
<text id="75601804">%null%</text>
<text id="75601805">%null%</text>
<text id="75601806">%null%</text>
<text id="75601807">%null%</text>
<text id="75601808">%null%</text>
<text id="75601809">%null%</text>
<text id="75601810">'Twould seem some sunlight-poisoned
creatures are worthy of great honour.</text>
<text id="75601811">%null%</text>
<text id="75601812">%null%</text>
<text id="75601813">%null%</text>
<text id="75601814">%null%</text>
<text id="75601815">%null%</text>
<text id="75601816">%null%</text>
<text id="75601817">%null%</text>
<text id="75601818">%null%</text>
<text id="75601819">%null%</text>
<text id="75601820">Thou'st permitted me to dream.
One day, rats and humans may yet live and die together, in peace.</text>
<text id="75605000">I will destroy thee, filthy human!</text>
<text id="75605001">%null%</text>
<text id="75605002">%null%</text>
<text id="75605003">%null%</text>
<text id="75605004">%null%</text>
<text id="75605005">%null%</text>
<text id="75605006">%null%</text>
<text id="75605007">%null%</text>
<text id="75605008">%null%</text>
<text id="75605009">%null%</text>
<text id="75605010">Thou art a fool to show thy face here!</text>
<text id="75605011">%null%</text>
<text id="75605012">%null%</text>
<text id="75605013">%null%</text>
<text id="75605014">%null%</text>
<text id="75605015">%null%</text>
