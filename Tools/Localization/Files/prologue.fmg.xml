<entries>
<text id="10000">%null%</text>
<text id="99900000">Perhaps you've seen it, maybe in a dream.</text>
<text id="99900001">%null%</text>
<text id="99900002">%null%</text>
<text id="99900003">%null%</text>
<text id="99900004">%null%</text>
<text id="99900005">%null%</text>
<text id="99900006">%null%</text>
<text id="99900007">%null%</text>
<text id="99900008">%null%</text>
<text id="99900009">%null%</text>
<text id="99900010">A murky, forgotten land.</text>
<text id="99900011">%null%</text>
<text id="99900012">%null%</text>
<text id="99900013">%null%</text>
<text id="99900014">%null%</text>
<text id="99900015">%null%</text>
<text id="99900016">%null%</text>
<text id="99900017">%null%</text>
<text id="99900018">%null%</text>
<text id="99900019">%null%</text>
<text id="99900020">A place where souls</text>
<text id="99900021">%null%</text>
<text id="99900022">%null%</text>
<text id="99900023">%null%</text>
<text id="99900024">%null%</text>
<text id="99900025">
may mend your ailing mind.</text>
<text id="99900026">%null%</text>
<text id="99900027">%null%</text>
<text id="99900028">%null%</text>
<text id="99900029">%null%</text>
<text id="99900030">You will lose everything.</text>
<text id="99900031">%null%</text>
<text id="99900032">%null%</text>
<text id="99900033">%null%</text>
<text id="99900034">%null%</text>
<text id="99900035">%null%</text>
<text id="99900036">%null%</text>
<text id="99900037">%null%</text>
<text id="99900038">%null%</text>
<text id="99900039">%null%</text>
<text id="99900040">Once branded.</text>
<text id="99900041">%null%</text>
<text id="99900042">%null%</text>
<text id="99900043">%null%</text>
<text id="99900044">%null%</text>
<text id="99900045">%null%</text>
<text id="99900046">%null%</text>
<text id="99900047">%null%</text>
<text id="99900048">%null%</text>
<text id="99900049">%null%</text>
<text id="99900050">The symbol of the curse.</text>
<text id="99900051">%null%</text>
<text id="99900052">%null%</text>
<text id="99900053">%null%</text>
<text id="99900054">%null%</text>
<text id="99900055">%null%</text>
<text id="99900056">%null%</text>
<text id="99900057">%null%</text>
<text id="99900058">%null%</text>
<text id="99900059">%null%</text>
<text id="99900060">An augur of darkness.</text>
<text id="99900061">%null%</text>
<text id="99900062">%null%</text>
<text id="99900063">%null%</text>
<text id="99900064">%null%</text>
<text id="99900065">%null%</text>
<text id="99900066">%null%</text>
<text id="99900067">%null%</text>
<text id="99900068">%null%</text>
<text id="99900069">%null%</text>
<text id="99900070">Your past. Your future.</text>
<text id="99900071">%null%</text>
<text id="99900072">%null%</text>
<text id="99900073">%null%</text>
<text id="99900074">%null%</text>
<text id="99900075">Your very light.</text>
<text id="99900076">%null%</text>
<text id="99900077">%null%</text>
<text id="99900078">%null%</text>
<text id="99900079">%null%</text>
<text id="99900080">None will have meaning, and you won't even care.</text>
<text id="99900081">%null%</text>
<text id="99900082">%null%</text>
<text id="99900083">%null%</text>
<text id="99900084">%null%</text>
<text id="99900085">%null%</text>
<text id="99900086">%null%</text>
<text id="99900087">%null%</text>
<text id="99900088">%null%</text>
<text id="99900089">%null%</text>
<text id="99900090">By then, you'll be something other than human.</text>
<text id="99900091">%null%</text>
<text id="99900092">%null%</text>
<text id="99900093">%null%</text>
<text id="99900094">%null%</text>
<text id="99900095">%null%</text>
<text id="99900096">%null%</text>
<text id="99900097">%null%</text>
<text id="99900098">%null%</text>
<text id="99900099">%null%</text>
<text id="99900100">A thing that feeds on souls.</text>
<text id="99900101">%null%</text>
<text id="99900102">%null%</text>
<text id="99900103">%null%</text>
<text id="99900104">%null%</text>
<text id="99900105">%null%</text>
<text id="99900106">%null%</text>
<text id="99900107">%null%</text>
<text id="99900108">%null%</text>
<text id="99900109">%null%</text>
<text id="99900110">A Hollow.</text>
<text id="99900111">%null%</text>
<text id="99900112">%null%</text>
<text id="99900113">%null%</text>
<text id="99900114">%null%</text>
<text id="99900115">%null%</text>
<text id="99900116">%null%</text>
<text id="99900117">%null%</text>
<text id="99900118">%null%</text>
<text id="99900119">%null%</text>
<text id="99900120">Long ago, in a walled off land, far to the north,</text>
<text id="99900121">%null%</text>
<text id="99900122">%null%</text>
<text id="99900123">%null%</text>
<text id="99900124">%null%</text>
<text id="99900125">a great king built a great kingdom.</text>
<text id="99900126">%null%</text>
<text id="99900127">%null%</text>
<text id="99900128">%null%</text>
<text id="99900129">%null%</text>
<text id="99900130">I believe they called it Drangleic.</text>
<text id="99900131">%null%</text>
<text id="99900132">%null%</text>
<text id="99900133">%null%</text>
<text id="99900134">%null%</text>
<text id="99900135">%null%</text>
<text id="99900136">%null%</text>
<text id="99900137">%null%</text>
<text id="99900138">%null%</text>
<text id="99900139">%null%</text>
<text id="99900140">Perhaps you're familiar.</text>
<text id="99900141">%null%</text>
<text id="99900142">%null%</text>
<text id="99900143">%null%</text>
<text id="99900144">%null%</text>
<text id="99900145">%null%</text>
<text id="99900146">%null%</text>
<text id="99900147">%null%</text>
<text id="99900148">%null%</text>
<text id="99900149">%null%</text>
<text id="99900150">No, how could you be.</text>
<text id="99900151">%null%</text>
<text id="99900152">%null%</text>
<text id="99900153">%null%</text>
<text id="99900154">%null%</text>
<text id="99900155">%null%</text>
<text id="99900156">%null%</text>
<text id="99900157">%null%</text>
<text id="99900158">%null%</text>
<text id="99900159">%null%</text>
<text id="99900160">But one day, you will stand before its decrepit gate.</text>
<text id="99900161">%null%</text>
<text id="99900162">%null%</text>
<text id="99900163">%null%</text>
<text id="99900164">%null%</text>
<text id="99900165">%null%</text>
<text id="99900166">%null%</text>
<text id="99900167">%null%</text>
<text id="99900168">%null%</text>
<text id="99900169">%null%</text>
<text id="99900170">Without really knowing why...</text>
<text id="99900171">%null%</text>
<text id="99900172">%null%</text>
<text id="99900173">%null%</text>
<text id="99900174">%null%</text>
<text id="99900175">%null%</text>
<text id="99900176">%null%</text>
<text id="99900177">%null%</text>
<text id="99900178">%null%</text>
<text id="99900179">%null%</text>
<text id="99900180">Like a moth drawn to a flame.</text>
<text id="99900181">%null%</text>
<text id="99900182">%null%</text>
<text id="99900183">%null%</text>
<text id="99900184">%null%</text>
<text id="99900185">%null%</text>
<text id="99900186">%null%</text>
<text id="99900187">%null%</text>
<text id="99900188">%null%</text>
<text id="99900189">%null%</text>
<text id="99900190">Your wings will burn in anguish.</text>
<text id="99900191">%null%</text>
<text id="99900192">%null%</text>
<text id="99900193">%null%</text>
<text id="99900194">%null%</text>
<text id="99900195">%null%</text>
<text id="99900196">%null%</text>
<text id="99900197">%null%</text>
<text id="99900198">%null%</text>
<text id="99900199">%null%</text>
<text id="99900200">Time after time.</text>
<text id="99900201">%null%</text>
<text id="99900202">%null%</text>
<text id="99900203">%null%</text>
<text id="99900204">%null%</text>
<text id="99900205">%null%</text>
<text id="99900206">%null%</text>
<text id="99900207">%null%</text>
<text id="99900208">%null%</text>
<text id="99900209">%null%</text>
<text id="99900210">For that is your fate.</text>
<text id="99900211">%null%</text>
<text id="99900212">%null%</text>
<text id="99900213">%null%</text>
<text id="99900214">%null%</text>
<text id="99900215">%null%</text>
<text id="99900216">%null%</text>
<text id="99900217">%null%</text>
<text id="99900218">%null%</text>
<text id="99900219">%null%</text>
<text id="99900220">The fate of the cursed.</text>
</entries>
