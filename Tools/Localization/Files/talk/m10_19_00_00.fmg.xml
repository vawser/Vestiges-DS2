<text id="75300817">%null%</text>
<text id="75300818">%null%</text>
<text id="75300819">%null%</text>
<text id="75300820">Hah hah, ooh hoo!</text>
<text id="75300900">Hah! Haaaah!</text>
<text id="75300901">%null%</text>
<text id="75300902">%null%</text>
<text id="75300903">%null%</text>
