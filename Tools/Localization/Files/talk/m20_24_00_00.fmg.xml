<text id="70005007">%null%</text>
<text id="70005008">%null%</text>
<text id="70005009">%null%</text>
<text id="70005010">(no line)</text>
<text id="70005011">%null%</text>
<text id="70005012">%null%</text>
<text id="70005013">%null%</text>
<text id="70005014">%null%</text>
<text id="70005015">%null%</text>
<text id="70005016">%null%</text>
<text id="70005017">%null%</text>
<text id="70005018">%null%</text>
<text id="70005019">%null%</text>
<text id="70005020">(no line)</text>
<text id="70005021">%null%</text>
<text id="70005022">%null%</text>
<text id="70005023">%null%</text>
<text id="70005024">%null%</text>
<text id="70005025">%null%</text>
<text id="70005026">%null%</text>
<text id="70005027">%null%</text>
<text id="70005028">%null%</text>
<text id="70005029">%null%</text>
<text id="70005030">What are you...?</text>
<text id="70005031">%null%</text>
<text id="70005032">%null%</text>
<text id="70005033">%null%</text>
<text id="70005034">%null%</text>
<text id="70005035">%null%</text>
