<text id="74405038">%null%</text>
<text id="74405039">%null%</text>
<text id="74405040">What the devil's...!</text>
<text id="74405041">%null%</text>
<text id="74405042">%null%</text>
<text id="74405043">%null%</text>
<text id="74405044">%null%</text>
