<text id="74405057">%null%</text>
<text id="74405058">%null%</text>
<text id="74405059">%null%</text>
<text id="74405060">Not too shabby... Heh heh...</text>
<text id="74405061">%null%</text>
<text id="74405062">%null%</text>
<text id="74405063">%null%</text>
<text id="74405064">%null%</text>
