<text id="75605027">%null%</text>
<text id="75605028">%null%</text>
<text id="75605029">%null%</text>
<text id="75605030">What is thy purpose?</text>
<text id="75605031">%null%</text>
<text id="75605032">%null%</text>
<text id="75605033">%null%</text>
<text id="75605034">%null%</text>
