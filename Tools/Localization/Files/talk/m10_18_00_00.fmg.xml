<text id="72600007">%null%</text>
<text id="72600008">%null%</text>
<text id="72600009">%null%</text>
<text id="72600010">I Gavlan.</text>
<text id="72600011">%null%</text>
<text id="72600012">%null%</text>
<text id="72600013">%null%</text>
<text id="72600014">%null%</text>
<text id="72600015">%null%</text>
<text id="72600016">%null%</text>
<text id="72600017">%null%</text>
<text id="72600018">%null%</text>
<text id="72600019">%null%</text>
<text id="72600020">Gavlan wheel? Gavlan deal.</text>
<text id="72600021">%null%</text>
<text id="72600022">%null%</text>
<text id="72600023">%null%</text>
<text id="72600024">%null%</text>
<text id="72600025">%null%</text>
<text id="72600026">%null%</text>
<text id="72600027">%null%</text>
<text id="72600028">%null%</text>
<text id="72600029">%null%</text>
<text id="72600030">Gavlan want soul.
Many many soul. Gah hah!</text>
<text id="72600031">%null%</text>
<text id="72600032">%null%</text>
<text id="72600033">%null%</text>
<text id="72600034">%null%</text>
<text id="72600035">%null%</text>
<text id="72600036">%null%</text>
<text id="72600037">%null%</text>
<text id="72600038">%null%</text>
<text id="72600039">%null%</text>
<text id="72600040">What you want?</text>
<text id="72600060">With Gavlan, you wheel?
You deal! Gah hah!</text>
<text id="72600100">What? You want?</text>
<text id="72600200">Umm...You want? Want what?</text>
<text id="72600300">Umm...Gavlan know you.</text>
<text id="72600400">Make deal with Gavlan?</text>
<text id="72600500">Oh Gavlan, know you.
What you want?</text>
<text id="72600600">You, again.
Gavlan, meet you again.</text>
<text id="72600700">Many deal...many thanks! Gah hah!</text>
<text id="72600800">You? Go home?</text>
<text id="72600900">Umm...</text>
<text id="72605000">Umm...You strong.</text>
<text id="72605001">%null%</text>
<text id="72605002">%null%</text>
<text id="72605003">%null%</text>
<text id="72605004">%null%</text>
<text id="72605005">%null%</text>
<text id="72605006">%null%</text>
<text id="72605007">%null%</text>
<text id="72605008">%null%</text>
<text id="72605009">%null%</text>
<text id="72605010">You die!</text>
<text id="72605011">%null%</text>
<text id="72605012">%null%</text>
<text id="72605013">%null%</text>
<text id="72605014">%null%</text>
<text id="72605015">%null%</text>
<text id="72605016">%null%</text>
<text id="72605017">%null%</text>
<text id="72605018">%null%</text>
<text id="72605019">%null%</text>
<text id="72605020">%null%</text>
<text id="72605021">%null%</text>
<text id="72605022">%null%</text>
<text id="72605023">%null%</text>
<text id="72605024">%null%</text>
<text id="72605025">%null%</text>
<text id="72605026">%null%</text>
<text id="72605027">%null%</text>
<text id="72605028">%null%</text>
<text id="72605029">%null%</text>
<text id="72605030">Umm?</text>
<text id="72605031">%null%</text>
<text id="72605032">%null%</text>
<text id="72605033">%null%</text>
<text id="72605034">%null%</text>
<text id="72605035">%null%</text>
<text id="72605036">%null%</text>
<text id="72605037">%null%</text>
<text id="72605038">%null%</text>
<text id="72605039">%null%</text>
<text id="72605040">%null%</text>
<text id="72605041">%null%</text>
<text id="72605042">%null%</text>
<text id="72605043">%null%</text>
<text id="72605044">%null%</text>
<text id="72605045">%null%</text>
<text id="72605046">%null%</text>
<text id="72605047">%null%</text>
<text id="72605048">%null%</text>
<text id="72605049">%null%</text>
<text id="72605050">%null%</text>
<text id="72605051">%null%</text>
<text id="72605052">%null%</text>
<text id="72605053">%null%</text>
<text id="72605054">%null%</text>
<text id="72605055">%null%</text>
<text id="72605056">%null%</text>
<text id="72605057">%null%</text>
<text id="72605058">%null%</text>
<text id="72605059">%null%</text>
<text id="72605060">%null%</text>
<text id="72605061">%null%</text>
<text id="72605062">%null%</text>
<text id="72605063">%null%</text>
<text id="72605064">%null%</text>
<text id="72605065">%null%</text>
<text id="72605066">%null%</text>
<text id="72605067">%null%</text>
<text id="72605068">%null%</text>
<text id="72605069">%null%</text>
<text id="72605070">%null%</text>
<text id="72606000">Wheel...deal...</text>
<text id="72607000">With Gavlan, you deal.
Much, much deal.</text>
<text id="72607001">%null%</text>
<text id="72607002">%null%</text>
<text id="72607003">%null%</text>
<text id="72607004">%null%</text>
<text id="72607005">%null%</text>
<text id="72607006">%null%</text>
<text id="72607007">%null%</text>
<text id="72607008">%null%</text>
<text id="72607009">%null%</text>
<text id="72607010">These, you take, ah
For you.</text>
<text id="75200000">What is it?</text>
<text id="75200001">%null%</text>
<text id="75200002">%null%</text>
<text id="75200003">%null%</text>
<text id="75200004">%null%</text>
<text id="75200005">%null%</text>
<text id="75200006">%null%</text>
<text id="75200007">%null%</text>
<text id="75200008">%null%</text>
<text id="75200009">%null%</text>
<text id="75200010">I don't know you, and you don't know me.</text>
<text id="75200011">%null%</text>
<text id="75200012">%null%</text>
<text id="75200013">%null%</text>
<text id="75200014">%null%</text>
<text id="75200015">%null%</text>
<text id="75200016">%null%</text>
<text id="75200017">%null%</text>
<text id="75200018">%null%</text>
<text id="75200019">%null%</text>
<text id="75200020">Things are better that way.</text>
<text id="75200100">Phew...</text>
<text id="75200101">%null%</text>
<text id="75200102">%null%</text>
<text id="75200103">%null%</text>
<text id="75200104">%null%</text>
<text id="75200105">%null%</text>
<text id="75200106">%null%</text>
<text id="75200107">%null%</text>
<text id="75200108">%null%</text>
<text id="75200109">%null%</text>
<text id="75200110">Heh heh. You are an odd one.</text>
<text id="75200111">%null%</text>
<text id="75200112">%null%</text>
<text id="75200113">%null%</text>
<text id="75200114">%null%</text>
<text id="75200115">%null%</text>
<text id="75200116">%null%</text>
<text id="75200117">%null%</text>
<text id="75200118">%null%</text>
<text id="75200119">%null%</text>
<text id="75200120">Normally, people keep a safe distance
when they see this mask. But you...</text>
<text id="75200121">%null%</text>
<text id="75200122">%null%</text>
<text id="75200123">%null%</text>
<text id="75200124">%null%</text>
<text id="75200125">%null%</text>
<text id="75200126">%null%</text>
<text id="75200127">%null%</text>
<text id="75200128">%null%</text>
<text id="75200129">%null%</text>
<text id="75200130">I am called Lucatiel.</text>
<text id="75200131">%null%</text>
<text id="75200132">%null%</text>
<text id="75200133">%null%</text>
<text id="75200134">%null%</text>
<text id="75200135">%null%</text>
<text id="75200136">%null%</text>
<text id="75200137">%null%</text>
<text id="75200138">%null%</text>
<text id="75200139">%null%</text>
<text id="75200140">From the land of Mirrah, to the far east.
across the mountains.</text>
<text id="75200200">They say Drangleic brims with powerful souls.</text>
<text id="75200201">%null%</text>
<text id="75200202">%null%</text>
<text id="75200203">%null%</text>
<text id="75200204">%null%</text>
<text id="75200205">%null%</text>
<text id="75200206">%null%</text>
<text id="75200207">%null%</text>
<text id="75200208">%null%</text>
<text id="75200209">%null%</text>
<text id="75200210">And so I came to claim my share.</text>
<text id="75200211">%null%</text>
<text id="75200212">%null%</text>
<text id="75200213">%null%</text>
<text id="75200214">%null%</text>
<text id="75200215">%null%</text>
<text id="75200216">%null%</text>
<text id="75200217">%null%</text>
<text id="75200218">%null%</text>
<text id="75200219">%null%</text>
<text id="75200220">But what a strange place...Even the rumours did not prepare me.</text>
<text id="75200300">You are an odd one, indeed.
I've always made a point of avoiding people.</text>
<text id="75200301">%null%</text>
<text id="75200302">%null%</text>
<text id="75200303">%null%</text>
<text id="75200304">%null%</text>
<text id="75200305">%null%</text>
<text id="75200306">%null%</text>
<text id="75200307">%null%</text>
<text id="75200308">%null%</text>
<text id="75200309">%null%</text>
<text id="75200310">While you've made a point of engaging me.</text>
<text id="75200311">%null%</text>
<text id="75200312">%null%</text>
<text id="75200313">%null%</text>
<text id="75200314">%null%</text>
<text id="75200315">%null%</text>
<text id="75200316">%null%</text>
<text id="75200317">%null%</text>
<text id="75200318">%null%</text>
<text id="75200319">%null%</text>
<text id="75200320">I can see that you are mid-journey.
If you require assistance, I will help you.</text>
<text id="75200321">%null%</text>
<text id="75200322">%null%</text>
<text id="75200323">%null%</text>
<text id="75200324">%null%</text>
<text id="75200325">%null%</text>
<text id="75200326">%null%</text>
<text id="75200327">%null%</text>
<text id="75200328">%null%</text>
<text id="75200329">%null%</text>
<text id="75200330">I come from Mirrah, a land of knights.
My sword is always ready.</text>
<text id="75200331">%null%</text>
<text id="75200332">%null%</text>
<text id="75200333">%null%</text>
<text id="75200334">%null%</text>
<text id="75200335">%null%</text>
<text id="75200336">%null%</text>
<text id="75200337">%null%</text>
<text id="75200338">%null%</text>
<text id="75200339">%null%</text>
<text id="75200340">Don't hesitate to call upon me.
Whatever happens, I won't be missed...</text>
<text id="75200500">I thought that might be you.
You haven't changed a bit, have you? Heh heh.</text>
<text id="75200501">%null%</text>
<text id="75200502">%null%</text>
<text id="75200503">%null%</text>
<text id="75200504">%null%</text>
<text id="75200505">%null%</text>
<text id="75200506">%null%</text>
<text id="75200507">%null%</text>
<text id="75200508">%null%</text>
<text id="75200509">%null%</text>
<text id="75200510">The longer I am here,
the more madness I discover.</text>
<text id="75200511">%null%</text>
<text id="75200512">%null%</text>
<text id="75200513">%null%</text>
<text id="75200514">%null%</text>
<text id="75200515">%null%</text>
<text id="75200516">%null%</text>
<text id="75200517">%null%</text>
<text id="75200518">%null%</text>
<text id="75200519">%null%</text>
<text id="75200520">A wretched place, indeed, but not 
without traces of its former glory.</text>
<text id="75200521">%null%</text>
<text id="75200522">%null%</text>
<text id="75200523">%null%</text>
<text id="75200524">%null%</text>
<text id="75200525">%null%</text>
<text id="75200526">%null%</text>
<text id="75200527">%null%</text>
<text id="75200528">%null%</text>
<text id="75200529">%null%</text>
<text id="75200530">What could have caused such degradation?</text>
<text id="75200600">Ah, yes, I have not thanked you
for humouring me the other day.</text>
<text id="75200601">%null%</text>
<text id="75200602">%null%</text>
<text id="75200603">%null%</text>
<text id="75200604">%null%</text>
<text id="75200605">%null%</text>
<text id="75200606">%null%</text>
<text id="75200607">%null%</text>
<text id="75200608">%null%</text>
<text id="75200609">%null%</text>
<text id="75200610">This is for you.</text>
<text id="75200611">%null%</text>
<text id="75200612">%null%</text>
<text id="75200613">%null%</text>
<text id="75200614">%null%</text>
<text id="75200615">%null%</text>
<text id="75200616">%null%</text>
<text id="75200617">%null%</text>
<text id="75200618">%null%</text>
<text id="75200619">%null%</text>
<text id="75200620">Of course, I've no idea what it is.
Heh heh.</text>
<text id="75200700">Our land of Mirrah is surrounded by enemies,
and constantly at war.</text>
<text id="75200701">%null%</text>
<text id="75200702">%null%</text>
<text id="75200703">%null%</text>
<text id="75200704">%null%</text>
<text id="75200705">%null%</text>
<text id="75200706">%null%</text>
<text id="75200707">%null%</text>
<text id="75200708">%null%</text>
<text id="75200709">%null%</text>
<text id="75200710">There is only one way up in Mirrah.
Join the Order, and prove yourself in battle.</text>
<text id="75200711">%null%</text>
<text id="75200712">%null%</text>
<text id="75200713">%null%</text>
<text id="75200714">%null%</text>
<text id="75200715">%null%</text>
<text id="75200716">%null%</text>
<text id="75200717">%null%</text>
<text id="75200718">%null%</text>
<text id="75200719">%null%</text>
<text id="75200720">My family had little fortune, and no name.
I had to carve out a piece of the world for myself...</text>
<text id="75200721">%null%</text>
<text id="75200722">%null%</text>
<text id="75200723">%null%</text>
<text id="75200724">%null%</text>
<text id="75200725">%null%</text>
<text id="75200726">%null%</text>
<text id="75200727">%null%</text>
<text id="75200728">%null%</text>
<text id="75200729">%null%</text>
<text id="75200730">...with two things.
My sword, and my loyalty to my Lord.</text>
<text id="75200800">I was raised to wield a sword from birth.</text>
<text id="75200801">%null%</text>
<text id="75200802">%null%</text>
<text id="75200803">%null%</text>
<text id="75200804">%null%</text>
<text id="75200805">%null%</text>
<text id="75200806">%null%</text>
<text id="75200807">%null%</text>
<text id="75200808">%null%</text>
<text id="75200809">%null%</text>
<text id="75200810">Life was hard,
but I never gave it a second thought.</text>
<text id="75200811">%null%</text>
<text id="75200812">%null%</text>
<text id="75200813">%null%</text>
<text id="75200814">%null%</text>
<text id="75200815">%null%</text>
<text id="75200816">%null%</text>
<text id="75200817">%null%</text>
<text id="75200818">%null%</text>
<text id="75200819">%null%</text>
<text id="75200820">I had swift success on the battlefield,
and quickly attained respectable stature.</text>
<text id="75200821">%null%</text>
<text id="75200822">%null%</text>
<text id="75200823">%null%</text>
<text id="75200824">%null%</text>
<text id="75200825">%null%</text>
<text id="75200826">%null%</text>
<text id="75200827">%null%</text>
<text id="75200828">%null%</text>
<text id="75200829">%null%</text>
<text id="75200830">And then I...</text>
<text id="75200831">%null%</text>
<text id="75200832">%null%</text>
<text id="75200833">%null%</text>
<text id="75200834">%null%</text>
<text id="75200835">%null%</text>
<text id="75200836">%null%</text>
<text id="75200837">%null%</text>
<text id="75200838">%null%</text>
<text id="75200839">%null%</text>
<text id="75200840">And then I came here...to...</text>
<text id="75200900">Have you heard of the Undead?
These poor souls affected by the curse.</text>
<text id="75200901">%null%</text>
<text id="75200902">%null%</text>
<text id="75200903">%null%</text>
<text id="75200904">%null%</text>
<text id="75200905">%null%</text>
<text id="75200906">%null%</text>
<text id="75200907">%null%</text>
<text id="75200908">%null%</text>
<text id="75200909">%null%</text>
<text id="75200910">An Undead gradually loses his humanity,
until his wits degrade completely.</text>
<text id="75200911">%null%</text>
<text id="75200912">%null%</text>
<text id="75200913">%null%</text>
<text id="75200914">%null%</text>
<text id="75200915">%null%</text>
<text id="75200916">%null%</text>
<text id="75200917">%null%</text>
<text id="75200918">%null%</text>
<text id="75200919">%null%</text>
<text id="75200920">Finally, he turns Hollow, and preys upon others.</text>
<text id="75200921">%null%</text>
<text id="75200922">%null%</text>
<text id="75200923">%null%</text>
<text id="75200924">%null%</text>
<text id="75200925">%null%</text>
<text id="75200926">%null%</text>
<text id="75200927">%null%</text>
<text id="75200928">%null%</text>
<text id="75200929">%null%</text>
<text id="75200930">And a Hollow can never be human again.</text>
<text id="75200931">%null%</text>
<text id="75200932">%null%</text>
<text id="75200933">%null%</text>
<text id="75200934">%null%</text>
<text id="75200935">%null%</text>
<text id="75200936">%null%</text>
<text id="75200937">%null%</text>
<text id="75200938">%null%</text>
<text id="75200939">%null%</text>
<text id="75200940">One can skirt this wicked fate
only with the help of the souls found here.</text>
<text id="75200941">%null%</text>
<text id="75200942">%null%</text>
<text id="75200943">%null%</text>
<text id="75200944">%null%</text>
<text id="75200945">%null%</text>
<text id="75200946">%null%</text>
<text id="75200947">%null%</text>
<text id="75200948">%null%</text>
<text id="75200949">%null%</text>
<text id="75200950">Assuming, of course, that the legends are true.</text>
<text id="75200951">%null%</text>
<text id="75200952">%null%</text>
<text id="75200953">%null%</text>
<text id="75200954">%null%</text>
<text id="75200955">%null%</text>
<text id="75200956">%null%</text>
<text id="75200957">%null%</text>
<text id="75200958">%null%</text>
<text id="75200959">%null%</text>
<text id="75200960">I can only hope...that they are.</text>
<text id="75201000">I'm sorry...to burden you with talk of my fate.</text>
<text id="75201001">%null%</text>
<text id="75201002">%null%</text>
<text id="75201003">%null%</text>
<text id="75201004">%null%</text>
<text id="75201005">%null%</text>
<text id="75201006">%null%</text>
<text id="75201007">%null%</text>
<text id="75201008">%null%</text>
<text id="75201009">%null%</text>
<text id="75201010">I suppose I've grown weary these days.</text>
<text id="75201100">Still on the road, are you?</text>
<text id="75201101">%null%</text>
<text id="75201102">%null%</text>
<text id="75201103">%null%</text>
<text id="75201104">%null%</text>
<text id="75201105">%null%</text>
<text id="75201106">%null%</text>
<text id="75201107">%null%</text>
<text id="75201108">%null%</text>
<text id="75201109">%null%</text>
<text id="75201110">Sorry to have burdened you so.</text>
<text id="75201111">%null%</text>
<text id="75201112">%null%</text>
<text id="75201113">%null%</text>
<text id="75201114">%null%</text>
<text id="75201115">%null%</text>
<text id="75201116">%null%</text>
<text id="75201117">%null%</text>
<text id="75201118">%null%</text>
<text id="75201119">%null%</text>
<text id="75201120">This is for you, by way of an apology.</text>
<text id="75201200">I've found my thoughts growing hazy.</text>
<text id="75201201">%null%</text>
<text id="75201202">%null%</text>
<text id="75201203">%null%</text>
<text id="75201204">%null%</text>
<text id="75201205">%null%</text>
<text id="75201206">%null%</text>
<text id="75201207">%null%</text>
<text id="75201208">%null%</text>
<text id="75201209">%null%</text>
<text id="75201210">My memories are fading, oldest first.</text>
<text id="75201211">%null%</text>
<text id="75201212">%null%</text>
<text id="75201213">%null%</text>
<text id="75201214">%null%</text>
<text id="75201215">%null%</text>
<text id="75201216">%null%</text>
<text id="75201217">%null%</text>
<text id="75201218">%null%</text>
<text id="75201219">%null%</text>
<text id="75201220">The curse is doing its work upon me.</text>
<text id="75201221">%null%</text>
<text id="75201222">%null%</text>
<text id="75201223">%null%</text>
<text id="75201224">%null%</text>
<text id="75201225">%null%</text>
<text id="75201226">%null%</text>
<text id="75201227">%null%</text>
<text id="75201228">%null%</text>
<text id="75201229">%null%</text>
<text id="75201230">I am frightened... Terribly so...</text>
<text id="75201231">%null%</text>
<text id="75201232">%null%</text>
<text id="75201233">%null%</text>
<text id="75201234">%null%</text>
<text id="75201235">%null%</text>
<text id="75201236">%null%</text>
<text id="75201237">%null%</text>
<text id="75201238">%null%</text>
<text id="75201239">%null%</text>
<text id="75201240">If everything should fade... What will be left of me...</text>
<text id="75201300">I had an older brother. We learned to fence together.</text>
<text id="75201301">%null%</text>
<text id="75201302">%null%</text>
<text id="75201303">%null%</text>
<text id="75201304">%null%</text>
<text id="75201305">%null%</text>
<text id="75201306">%null%</text>
<text id="75201307">%null%</text>
<text id="75201308">%null%</text>
<text id="75201309">%null%</text>
<text id="75201310">He became the most decorated
swordsman in all of Mirrah.</text>
<text id="75201311">%null%</text>
<text id="75201312">%null%</text>
<text id="75201313">%null%</text>
<text id="75201314">%null%</text>
<text id="75201315">%null%</text>
<text id="75201316">%null%</text>
<text id="75201317">%null%</text>
<text id="75201318">%null%</text>
<text id="75201319">%null%</text>
<text id="75201320">I never even compared to him.</text>
<text id="75201321">%null%</text>
<text id="75201322">%null%</text>
<text id="75201323">%null%</text>
<text id="75201324">%null%</text>
<text id="75201325">%null%</text>
<text id="75201326">%null%</text>
<text id="75201327">%null%</text>
<text id="75201328">%null%</text>
<text id="75201329">%null%</text>
<text id="75201330">In fact, I never beat him, not once.</text>
<text id="75201331">%null%</text>
<text id="75201332">%null%</text>
<text id="75201333">%null%</text>
<text id="75201334">%null%</text>
<text id="75201335">%null%</text>
<text id="75201336">%null%</text>
<text id="75201337">%null%</text>
<text id="75201338">%null%</text>
<text id="75201339">%null%</text>
<text id="75201340">But then, one day...he was gone,
lost without a trace.</text>
<text id="75201341">%null%</text>
<text id="75201342">%null%</text>
<text id="75201343">%null%</text>
<text id="75201344">%null%</text>
<text id="75201345">%null%</text>
<text id="75201346">%null%</text>
<text id="75201347">%null%</text>
<text id="75201348">%null%</text>
<text id="75201349">%null%</text>
<text id="75201350">Now I'm certain. That he was taken, by the curse.</text>
<text id="75201400">If only someone would hear my tale...</text>
<text id="75201401">%null%</text>
<text id="75201402">%null%</text>
<text id="75201403">%null%</text>
<text id="75201404">%null%</text>
<text id="75201405">%null%</text>
<text id="75201406">%null%</text>
<text id="75201407">%null%</text>
<text id="75201408">%null%</text>
<text id="75201409">%null%</text>
<text id="75201410">My brother must have come here, too.</text>
<text id="75201411">%null%</text>
<text id="75201412">%null%</text>
<text id="75201413">%null%</text>
<text id="75201414">%null%</text>
<text id="75201415">%null%</text>
<text id="75201416">%null%</text>
<text id="75201417">%null%</text>
<text id="75201418">%null%</text>
<text id="75201419">%null%</text>
<text id="75201420">Soon, I may forget even about him...</text>
<text id="75201500">Oh... You...</text>
<text id="75201501">%null%</text>
<text id="75201502">%null%</text>
<text id="75201503">%null%</text>
<text id="75201504">%null%</text>
<text id="75201505">%null%</text>
<text id="75201506">%null%</text>
<text id="75201507">%null%</text>
<text id="75201508">%null%</text>
<text id="75201509">%null%</text>
<text id="75201510">My thoughts...are very...scattered...</text>
<text id="75201600">What is this curse?</text>
<text id="75201601">%null%</text>
<text id="75201602">%null%</text>
<text id="75201603">%null%</text>
<text id="75201604">%null%</text>
<text id="75201605">%null%</text>
<text id="75201606">%null%</text>
<text id="75201607">%null%</text>
<text id="75201608">%null%</text>
<text id="75201609">%null%</text>
<text id="75201610">The question rings in my mind,
but I haven't the focus to answer it.</text>
<text id="75201700">Loss frightens me no end.
Loss of memory, loss of self.</text>
<text id="75201701">%null%</text>
<text id="75201702">%null%</text>
<text id="75201703">%null%</text>
<text id="75201704">%null%</text>
<text id="75201705">%null%</text>
<text id="75201706">%null%</text>
<text id="75201707">%null%</text>
<text id="75201708">%null%</text>
<text id="75201709">%null%</text>
<text id="75201710">If I were told that by killing you,
I would be freed of this curse...</text>
<text id="75201711">%null%</text>
<text id="75201712">%null%</text>
<text id="75201713">%null%</text>
<text id="75201714">%null%</text>
<text id="75201715">%null%</text>
<text id="75201716">%null%</text>
<text id="75201717">%null%</text>
<text id="75201718">%null%</text>
<text id="75201719">%null%</text>
<text id="75201720">Then I would draw my sword without hesitation.</text>
<text id="75201721">%null%</text>
<text id="75201722">%null%</text>
<text id="75201723">%null%</text>
<text id="75201724">%null%</text>
<text id="75201725">%null%</text>
<text id="75201726">%null%</text>
<text id="75201727">%null%</text>
<text id="75201728">%null%</text>
<text id="75201729">%null%</text>
<text id="75201730">I don't want to die, I want to exist.</text>
<text id="75201731">%null%</text>
<text id="75201732">%null%</text>
<text id="75201733">%null%</text>
<text id="75201734">%null%</text>
<text id="75201735">%null%</text>
<text id="75201736">%null%</text>
<text id="75201737">%null%</text>
<text id="75201738">%null%</text>
<text id="75201739">%null%</text>
<text id="75201740">I would sacrifice anything, anything at all for this.</text>
<text id="75201741">%null%</text>
<text id="75201742">%null%</text>
<text id="75201743">%null%</text>
<text id="75201744">%null%</text>
<text id="75201745">%null%</text>
<text id="75201746">%null%</text>
<text id="75201747">%null%</text>
<text id="75201748">%null%</text>
<text id="75201749">%null%</text>
<text id="75201750">It shames me, but it is the truth.</text>
<text id="75201800">Sometimes, I feel obsessed...
with this insignificant thing called "self".</text>
<text id="75201801">%null%</text>
<text id="75201802">%null%</text>
<text id="75201803">%null%</text>
<text id="75201804">%null%</text>
<text id="75201805">%null%</text>
<text id="75201806">%null%</text>
<text id="75201807">%null%</text>
<text id="75201808">%null%</text>
<text id="75201809">%null%</text>
<text id="75201810">But even so, I am compelled to preserve it.</text>
<text id="75201811">%null%</text>
<text id="75201812">%null%</text>
<text id="75201813">%null%</text>
<text id="75201814">%null%</text>
<text id="75201815">%null%</text>
<text id="75201816">%null%</text>
<text id="75201817">%null%</text>
<text id="75201818">%null%</text>
<text id="75201819">%null%</text>
<text id="75201820">Am I wrong to feel so?
Surely you'd do the same, in my shoes?</text>
<text id="75201821">%null%</text>
<text id="75201822">%null%</text>
<text id="75201823">%null%</text>
<text id="75201824">%null%</text>
<text id="75201825">%null%</text>
<text id="75201826">%null%</text>
<text id="75201827">%null%</text>
<text id="75201828">%null%</text>
<text id="75201829">%null%</text>
<text id="75201830">...</text>
<text id="75201831">%null%</text>
<text id="75201832">%null%</text>
<text id="75201833">%null%</text>
<text id="75201834">%null%</text>
<text id="75201835">%null%</text>
<text id="75201836">%null%</text>
<text id="75201837">%null%</text>
<text id="75201838">%null%</text>
<text id="75201839">%null%</text>
<text id="75201840">Maybe we're all cursed...</text>
<text id="75201841">%null%</text>
<text id="75201842">%null%</text>
<text id="75201843">%null%</text>
<text id="75201844">%null%</text>
<text id="75201845">%null%</text>
<text id="75201846">%null%</text>
<text id="75201847">%null%</text>
<text id="75201848">%null%</text>
<text id="75201849">%null%</text>
<text id="75201850">From the moment we're born...</text>
<text id="75205000">If this is your wish, so be it.</text>
<text id="75205001">%null%</text>
<text id="75205002">%null%</text>
<text id="75205003">%null%</text>
<text id="75205004">%null%</text>
<text id="75205005">%null%</text>
<text id="75205006">%null%</text>
<text id="75205007">%null%</text>
<text id="75205008">%null%</text>
<text id="75205009">%null%</text>
<text id="75205010">If you really insist...</text>
<text id="75205011">%null%</text>
<text id="75205012">%null%</text>
<text id="75205013">%null%</text>
<text id="75205014">%null%</text>
<text id="75205015">%null%</text>
<text id="75205016">%null%</text>
<text id="75205017">%null%</text>
<text id="75205018">%null%</text>
<text id="75205019">%null%</text>
<text id="75205020">Hmph... How sickening...</text>
<text id="75205021">%null%</text>
<text id="75205022">%null%</text>
<text id="75205023">%null%</text>
<text id="75205024">%null%</text>
<text id="75205025">%null%</text>
<text id="75205026">%null%</text>
<text id="75205027">%null%</text>
<text id="75205028">%null%</text>
<text id="75205029">%null%</text>
<text id="75205030">Eek!</text>
<text id="75205031">%null%</text>
<text id="75205032">%null%</text>
<text id="75205033">%null%</text>
<text id="75205034">%null%</text>
<text id="75205035">%null%</text>
<text id="75205036">%null%</text>
<text id="75205037">%null%</text>
<text id="75205038">%null%</text>
<text id="75205039">%null%</text>
<text id="75205040">What's become of you!</text>
<text id="75205041">%null%</text>
<text id="75205042">%null%</text>
<text id="75205043">%null%</text>
<text id="75205044">%null%</text>
<text id="75205045">%null%</text>
<text id="75205046">%null%</text>
<text id="75205047">%null%</text>
<text id="75205048">%null%</text>
<text id="75205049">%null%</text>
<text id="75205050">That will be quite enough!</text>
<text id="75205051">%null%</text>
<text id="75205052">%null%</text>
<text id="75205053">%null%</text>
<text id="75205054">%null%</text>
<text id="75205055">%null%</text>
<text id="75205056">%null%</text>
<text id="75205057">%null%</text>
<text id="75205058">%null%</text>
<text id="75205059">%null%</text>
<text id="75205060">I won't die in this foul place!</text>
<text id="75205061">%null%</text>
<text id="75205062">%null%</text>
<text id="75205063">%null%</text>
<text id="75205064">%null%</text>
<text id="75205065">%null%</text>
<text id="75205066">%null%</text>
<text id="75205067">%null%</text>
<text id="75205068">%null%</text>
<text id="75205069">%null%</text>
<text id="75205070">Oh... My dear brother...</text>
<text id="75206000">Please...find my brother...</text>
<text id="75207000">Please take these.</text>
<text id="75207001">%null%</text>
<text id="75207002">%null%</text>
<text id="75207003">%null%</text>
<text id="75207004">%null%</text>
<text id="75207005">%null%</text>
<text id="75207006">%null%</text>
<text id="75207007">%null%</text>
<text id="75207008">%null%</text>
<text id="75207009">%null%</text>
<text id="75207010">Consider this thanks,
for keeping me sane...</text>
<text id="76600000">Hmm...I sense power...</text>
<text id="76600001">%null%</text>
<text id="76600002">%null%</text>
<text id="76600003">%null%</text>
<text id="76600004">%null%</text>
<text id="76600005">%null%</text>
<text id="76600006">%null%</text>
<text id="76600007">%null%</text>
<text id="76600008">%null%</text>
<text id="76600009">%null%</text>
<text id="76600010">Very well. From this day,
you shall be my new pupil.</text>
<text id="76600011">%null%</text>
<text id="76600012">%null%</text>
<text id="76600013">%null%</text>
<text id="76600014">%null%</text>
<text id="76600015">%null%</text>
<text id="76600016">%null%</text>
<text id="76600017">%null%</text>
<text id="76600018">%null%</text>
<text id="76600019">%null%</text>
<text id="76600020">I am Carhillion. Carhillion of the Fold.
Surely you've heard the name?</text>
<text id="76600021">%null%</text>
<text id="76600022">%null%</text>
<text id="76600023">%null%</text>
<text id="76600024">%null%</text>
<text id="76600025">%null%</text>
<text id="76600026">%null%</text>
<text id="76600027">%null%</text>
<text id="76600028">%null%</text>
<text id="76600029">%null%</text>
<text id="76600030">Now, now, do not be afraid.
Let us explore the limits of sorcery.</text>
<text id="76600100">Hmm...</text>
<text id="76600101">%null%</text>
<text id="76600102">%null%</text>
<text id="76600103">%null%</text>
<text id="76600104">%null%</text>
<text id="76600105">%null%</text>
<text id="76600106">%null%</text>
<text id="76600107">%null%</text>
<text id="76600108">%null%</text>
<text id="76600109">%null%</text>
<text id="76600110">I see light, but it remains dim...</text>
<text id="76600111">%null%</text>
<text id="76600112">%null%</text>
<text id="76600113">%null%</text>
<text id="76600114">%null%</text>
<text id="76600115">%null%</text>
<text id="76600116">%null%</text>
<text id="76600117">%null%</text>
<text id="76600118">%null%</text>
<text id="76600119">%null%</text>
<text id="76600120">I am Carhillion, and I've no
interest in the magic-impaired.</text>
<text id="76601000">Do you seek my teachings?
Very well, very well.</text>
<text id="76601100">Back already?
Your diligence is commendable.</text>
<text id="76601200">Your visit is welcome.
I trust you've kept up your studies.</text>
<text id="76601300">Oh, there you are. Do you seek my teachings?</text>
<text id="76601400">Young pupil, do not take my teachings lightly.</text>
<text id="76601500">One day, my teachings will save you.</text>
<text id="76601600">Young pupil, may we meet again, in Majula.</text>
<text id="76601700">The path is yet long, young pupil.</text>
<text id="76602000">The forces of magic, and souls, 
lie dormant in this land.</text>
<text id="76602001">%null%</text>
<text id="76602002">%null%</text>
<text id="76602003">%null%</text>
<text id="76602004">%null%</text>
<text id="76602005">%null%</text>
<text id="76602006">%null%</text>
<text id="76602007">%null%</text>
<text id="76602008">%null%</text>
<text id="76602009">%null%</text>
<text id="76602010">It is a fitting place
to hone my sorceries.</text>
<text id="76602011">%null%</text>
<text id="76602012">%null%</text>
<text id="76602013">%null%</text>
<text id="76602014">%null%</text>
<text id="76602015">%null%</text>
<text id="76602016">%null%</text>
<text id="76602017">%null%</text>
<text id="76602018">%null%</text>
<text id="76602019">%null%</text>
<text id="76602020">I suspect that I may even
conjure up new spells here.</text>
<text id="76602021">%null%</text>
<text id="76602022">%null%</text>
<text id="76602023">%null%</text>
<text id="76602024">%null%</text>
<text id="76602025">%null%</text>
<text id="76602026">%null%</text>
<text id="76602027">%null%</text>
<text id="76602028">%null%</text>
<text id="76602029">%null%</text>
<text id="76602030">Sorcery is yet a mystery, even to me.
Let us mature together, young pupil.</text>
<text id="76602100">Use scrolls to unleash
the power of sorceries.</text>
<text id="76602101">%null%</text>
<text id="76602102">%null%</text>
<text id="76602103">%null%</text>
<text id="76602104">%null%</text>
<text id="76602105">%null%</text>
<text id="76602106">%null%</text>
<text id="76602107">%null%</text>
<text id="76602108">%null%</text>
<text id="76602109">%null%</text>
<text id="76602110">But the power of each scroll
is reliant on that of its owner.</text>
<text id="76602111">%null%</text>
<text id="76602112">%null%</text>
<text id="76602113">%null%</text>
<text id="76602114">%null%</text>
<text id="76602115">%null%</text>
<text id="76602116">%null%</text>
<text id="76602117">%null%</text>
<text id="76602118">%null%</text>
<text id="76602119">%null%</text>
<text id="76602120">The untrained cannot wield
a scroll to its proper potential.</text>
<text id="76602121">%null%</text>
<text id="76602122">%null%</text>
<text id="76602123">%null%</text>
<text id="76602124">%null%</text>
<text id="76602125">%null%</text>
<text id="76602126">%null%</text>
<text id="76602127">%null%</text>
<text id="76602128">%null%</text>
<text id="76602129">%null%</text>
<text id="76602130">Never forget, young pupil...
There are no shortcuts to mastery.</text>
<text id="76605000">The arrogance of the young!</text>
<text id="76605001">%null%</text>
<text id="76605002">%null%</text>
<text id="76605003">%null%</text>
<text id="76605004">%null%</text>
<text id="76605005">%null%</text>
<text id="76605006">%null%</text>
<text id="76605007">%null%</text>
<text id="76605008">%null%</text>
<text id="76605009">%null%</text>
<text id="76605010">Behold my true power.</text>
<text id="76605011">%null%</text>
<text id="76605012">%null%</text>
<text id="76605013">%null%</text>
<text id="76605014">%null%</text>
<text id="76605015">%null%</text>
<text id="76605016">%null%</text>
<text id="76605017">%null%</text>
<text id="76605018">%null%</text>
<text id="76605019">%null%</text>
<text id="76605020">How very tragic...</text>
