<text id="75605017">%null%</text>
<text id="75605018">%null%</text>
<text id="75605019">%null%</text>
<text id="75605020">Disgusting creature...</text>
<text id="75605021">%null%</text>
<text id="75605022">%null%</text>
<text id="75605023">%null%</text>
<text id="75605024">%null%</text>
