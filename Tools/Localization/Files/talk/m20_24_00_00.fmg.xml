<text id="70005037">%null%</text>
<text id="70005038">%null%</text>
<text id="70005039">%null%</text>
<text id="70005040">(no line)</text>
<text id="70005041">%null%</text>
<text id="70005042">%null%</text>
<text id="70005043">%null%</text>
<text id="70005044">%null%</text>
<text id="70005045">%null%</text>
<text id="70005046">%null%</text>
<text id="70005047">%null%</text>
<text id="70005048">%null%</text>
<text id="70005049">%null%</text>
<text id="70005050">(no line)</text>
<text id="70005051">%null%</text>
