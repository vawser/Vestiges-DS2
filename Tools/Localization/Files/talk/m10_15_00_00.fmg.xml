<text id="70004200">Bearer of the curse.</text>
<text id="70004201">%null%</text>
<text id="70004202">%null%</text>
<text id="70004203">%null%</text>
<text id="70004204">%null%</text>
<text id="70004205">%null%</text>
<text id="70004206">%null%</text>
<text id="70004207">%null%</text>
<text id="70004208">%null%</text>
<text id="70004209">%null%</text>
<text id="70004210">Long have I awaited one such as you,
one who might shatter the shackles of fate.</text>
<text id="70004211">%null%</text>
<text id="70004212">%null%</text>
<text id="70004213">%null%</text>
<text id="70004214">%null%</text>
<text id="70004215">%null%</text>
<text id="70004216">%null%</text>
<text id="70004217">%null%</text>
<text id="70004218">%null%</text>
<text id="70004219">%null%</text>
<text id="70004220">One who can set me free.</text>
<text id="70004221">%null%</text>
<text id="70004222">%null%</text>
<text id="70004223">%null%</text>
<text id="70004224">%null%</text>
<text id="70004225">%null%</text>
<text id="70004226">%null%</text>
<text id="70004227">%null%</text>
<text id="70004228">%null%</text>
<text id="70004229">%null%</text>
<text id="70004230">Bearer of the curse,
it was my own manifestation that led you here.</text>
<text id="70004231">%null%</text>
<text id="70004232">%null%</text>
<text id="70004233">%null%</text>
<text id="70004234">%null%</text>
<text id="70004235">%null%</text>
<text id="70004236">%null%</text>
<text id="70004237">%null%</text>
<text id="70004238">%null%</text>
<text id="70004239">%null%</text>
<text id="70004240">The ancient dragon has watched over the world for aeons past.</text>
<text id="70004241">%null%</text>
<text id="70004242">%null%</text>
<text id="70004243">%null%</text>
<text id="70004244">%null%</text>
<text id="70004245">%null%</text>
<text id="70004246">%null%</text>
<text id="70004247">%null%</text>
<text id="70004248">%null%</text>
<text id="70004249">%null%</text>
<text id="70004250">Take this.</text>
<text id="70004251">%null%</text>
<text id="70004252">%null%</text>
<text id="70004253">%null%</text>
<text id="70004254">%null%</text>
<text id="70004255">%null%</text>
<text id="70004256">%null%</text>
<text id="70004257">%null%</text>
<text id="70004258">%null%</text>
<text id="70004259">%null%</text>
<text id="70004260">Do not resist. The dragon welcomes you.</text>
<text id="70004261">%null%</text>
<text id="70004262">%null%</text>
<text id="70004263">%null%</text>
<text id="70004264">%null%</text>
<text id="70004265">%null%</text>
<text id="70004266">%null%</text>
<text id="70004267">%null%</text>
<text id="70004268">%null%</text>
<text id="70004269">%null%</text>
<text id="70004270">Now the throne lies within your grasp,
surely the dragon will speak to you.</text>
<text id="70004300">Listen to what the dragon says.</text>
<text id="70004301">%null%</text>
<text id="70004302">%null%</text>
<text id="70004303">%null%</text>
<text id="70004304">%null%</text>
<text id="70004305">%null%</text>
<text id="70004306">%null%</text>
<text id="70004307">%null%</text>
<text id="70004308">%null%</text>
<text id="70004309">%null%</text>
<text id="70004310">With the throne so close at hand,
the dragon is sure to speak.</text>
<text id="70005000">%null%</text>
<text id="70005001">%null%</text>
<text id="70005002">%null%</text>
<text id="70005003">%null%</text>
<text id="70005004">%null%</text>
<text id="70005005">%null%</text>
<text id="70005006">%null%</text>
<text id="70005007">%null%</text>
<text id="70005008">%null%</text>
<text id="70005009">%null%</text>
<text id="70005010">%null%</text>
<text id="70005011">%null%</text>
<text id="70005012">%null%</text>
<text id="70005013">%null%</text>
<text id="70005014">%null%</text>
<text id="70005015">%null%</text>
<text id="70005016">%null%</text>
<text id="70005017">%null%</text>
<text id="70005018">%null%</text>
<text id="70005019">%null%</text>
<text id="70005020">%null%</text>
<text id="70005021">%null%</text>
<text id="70005022">%null%</text>
<text id="70005023">%null%</text>
<text id="70005024">%null%</text>
<text id="70005025">%null%</text>
<text id="70005026">%null%</text>
<text id="70005027">%null%</text>
<text id="70005028">%null%</text>
<text id="70005029">%null%</text>
<text id="70005030">What are you...?</text>
<text id="70005031">%null%</text>
<text id="70005032">%null%</text>
<text id="70005033">%null%</text>
<text id="70005034">%null%</text>
<text id="70005035">%null%</text>
<text id="70005036">%null%</text>
<text id="70005037">%null%</text>
<text id="70005038">%null%</text>
<text id="70005039">%null%</text>
<text id="70005040">%null%</text>
<text id="70005041">%null%</text>
<text id="70005042">%null%</text>
<text id="70005043">%null%</text>
<text id="70005044">%null%</text>
<text id="70005045">%null%</text>
<text id="70005046">%null%</text>
<text id="70005047">%null%</text>
<text id="70005048">%null%</text>
<text id="70005049">%null%</text>
<text id="70005050">%null%</text>
<text id="70005051">%null%</text>
<text id="70005052">%null%</text>
<text id="70005053">%null%</text>
<text id="70005054">%null%</text>
<text id="70005055">%null%</text>
<text id="70005056">%null%</text>
<text id="70005057">%null%</text>
<text id="70005058">%null%</text>
<text id="70005059">%null%</text>
<text id="70005060">If that is your wish...</text>
<text id="70005061">%null%</text>
<text id="70005062">%null%</text>
<text id="70005063">%null%</text>
<text id="70005064">%null%</text>
<text id="70005065">%null%</text>
<text id="70005066">%null%</text>
<text id="70005067">%null%</text>
<text id="70005068">%null%</text>
<text id="70005069">%null%</text>
<text id="70005070">...</text>
<text id="70006000">Bring an end to your journey...and mine.</text>
<text id="70009999">...</text>
<text id="75201900">Who are you...</text>
<text id="75201901">%null%</text>
<text id="75201902">%null%</text>
<text id="75201903">%null%</text>
<text id="75201904">%null%</text>
<text id="75201905">%null%</text>
<text id="75201906">%null%</text>
<text id="75201907">%null%</text>
<text id="75201908">%null%</text>
<text id="75201909">%null%</text>
<text id="75201910">Oh...No, forgive me...
I know you... Yes, of course.</text>
<text id="75202000">How goes your journey?</text>
<text id="75202001">%null%</text>
<text id="75202002">%null%</text>
<text id="75202003">%null%</text>
<text id="75202004">%null%</text>
<text id="75202005">%null%</text>
<text id="75202006">%null%</text>
<text id="75202007">%null%</text>
<text id="75202008">%null%</text>
<text id="75202009">%null%</text>
<text id="75202010">I know not what you seek
in this far-away land...</text>
<text id="75202011">%null%</text>
<text id="75202012">%null%</text>
<text id="75202013">%null%</text>
<text id="75202014">%null%</text>
<text id="75202015">%null%</text>
<text id="75202016">%null%</text>
<text id="75202017">%null%</text>
<text id="75202018">%null%</text>
<text id="75202019">%null%</text>
<text id="75202020">But I pray for your safety.</text>
<text id="75202100">My name is Lucatiel.</text>
<text id="75202101">%null%</text>
<text id="75202102">%null%</text>
<text id="75202103">%null%</text>
<text id="75202104">%null%</text>
<text id="75202105">%null%</text>
<text id="75202106">%null%</text>
<text id="75202107">%null%</text>
<text id="75202108">%null%</text>
<text id="75202109">%null%</text>
<text id="75202110">I beg of you, remember my name.</text>
<text id="75202111">%null%</text>
<text id="75202112">%null%</text>
<text id="75202113">%null%</text>
<text id="75202114">%null%</text>
<text id="75202115">%null%</text>
<text id="75202116">%null%</text>
<text id="75202117">%null%</text>
<text id="75202118">%null%</text>
<text id="75202119">%null%</text>
<text id="75202120">For I may not myself...</text>
<text id="75205000">If this is your wish, so be it.</text>
<text id="75205001">%null%</text>
<text id="75205002">%null%</text>
<text id="75205003">%null%</text>
<text id="75205004">%null%</text>
<text id="75205005">%null%</text>
<text id="75205006">%null%</text>
<text id="75205007">%null%</text>
<text id="75205008">%null%</text>
<text id="75205009">%null%</text>
<text id="75205010">If you really insist...</text>
<text id="75205011">%null%</text>
<text id="75205012">%null%</text>
<text id="75205013">%null%</text>
<text id="75205014">%null%</text>
<text id="75205015">%null%</text>
<text id="75205016">%null%</text>
<text id="75205017">%null%</text>
<text id="75205018">%null%</text>
<text id="75205019">%null%</text>
<text id="75205020">Hmph... How sickening...</text>
<text id="75205021">%null%</text>
<text id="75205022">%null%</text>
<text id="75205023">%null%</text>
<text id="75205024">%null%</text>
<text id="75205025">%null%</text>
<text id="75205026">%null%</text>
<text id="75205027">%null%</text>
<text id="75205028">%null%</text>
<text id="75205029">%null%</text>
<text id="75205030">Eek!</text>
<text id="75205031">%null%</text>
<text id="75205032">%null%</text>
<text id="75205033">%null%</text>
<text id="75205034">%null%</text>
<text id="75205035">%null%</text>
<text id="75205036">%null%</text>
<text id="75205037">%null%</text>
<text id="75205038">%null%</text>
<text id="75205039">%null%</text>
<text id="75205040">What's become of you!</text>
<text id="75205041">%null%</text>
<text id="75205042">%null%</text>
<text id="75205043">%null%</text>
<text id="75205044">%null%</text>
<text id="75205045">%null%</text>
<text id="75205046">%null%</text>
<text id="75205047">%null%</text>
<text id="75205048">%null%</text>
<text id="75205049">%null%</text>
<text id="75205050">That will be quite enough!</text>
<text id="75205051">%null%</text>
<text id="75205052">%null%</text>
<text id="75205053">%null%</text>
<text id="75205054">%null%</text>
<text id="75205055">%null%</text>
<text id="75205056">%null%</text>
<text id="75205057">%null%</text>
<text id="75205058">%null%</text>
<text id="75205059">%null%</text>
<text id="75205060">I won't die in this foul place!</text>
<text id="75205061">%null%</text>
<text id="75205062">%null%</text>
<text id="75205063">%null%</text>
<text id="75205064">%null%</text>
<text id="75205065">%null%</text>
<text id="75205066">%null%</text>
<text id="75205067">%null%</text>
<text id="75205068">%null%</text>
<text id="75205069">%null%</text>
<text id="75205070">Oh...my dear brother...</text>
<text id="75206000">Please...find my brother...</text>
<text id="75207000">Please take these.</text>
<text id="75207001">%null%</text>
<text id="75207002">%null%</text>
<text id="75207003">%null%</text>
<text id="75207004">%null%</text>
<text id="75207005">%null%</text>
<text id="75207006">%null%</text>
<text id="75207007">%null%</text>
<text id="75207008">%null%</text>
<text id="75207009">%null%</text>
<text id="75207010">Consider this thanks,
for keeping me sane...</text>
<text id="77100000">P-please, just stay away.</text>
<text id="77100001">%null%</text>
<text id="77100002">%null%</text>
<text id="77100003">%null%</text>
<text id="77100004">%null%</text>
<text id="77100005">%null%</text>
<text id="77100006">%null%</text>
<text id="77100007">%null%</text>
<text id="77100008">%null%</text>
<text id="77100009">%null%</text>
<text id="77100010">No, please, don't come near me.
Nothing good will come of it.</text>
<text id="77100100">Just leave me alone, please.</text>
<text id="77100101">%null%</text>
<text id="77100102">%null%</text>
<text id="77100103">%null%</text>
<text id="77100104">%null%</text>
<text id="77100105">%null%</text>
<text id="77100106">%null%</text>
<text id="77100107">%null%</text>
<text id="77100108">%null%</text>
<text id="77100109">%null%</text>
<text id="77100110">Leave this place, and leave me be,
as I'd not see any harm befall you.</text>
<text id="77100200">Please, just leave me alone.</text>
<text id="77100300">I haven't said anything strange, have I?</text>
<text id="77100301">%null%</text>
<text id="77100302">%null%</text>
<text id="77100303">%null%</text>
<text id="77100304">%null%</text>
<text id="77100305">%null%</text>
<text id="77100306">%null%</text>
<text id="77100307">%null%</text>
<text id="77100308">%null%</text>
<text id="77100309">%null%</text>
<text id="77100310">I have absolutely no intention of leaving here.</text>
<text id="77100311">%null%</text>
<text id="77100312">%null%</text>
<text id="77100313">%null%</text>
<text id="77100314">%null%</text>
<text id="77100315">%null%</text>
<text id="77100316">%null%</text>
<text id="77100317">%null%</text>
<text id="77100318">%null%</text>
<text id="77100319">%null%</text>
<text id="77100320">Do not attempt to help me.
Just let me sit here, and wither quietly away...</text>
<text id="77100400">Well, you're nicely hollowed, aren't you...</text>
<text id="77100401">%null%</text>
<text id="77100402">%null%</text>
<text id="77100403">%null%</text>
<text id="77100404">%null%</text>
<text id="77100405">%null%</text>
<text id="77100406">%null%</text>
<text id="77100407">%null%</text>
<text id="77100408">%null%</text>
<text id="77100409">%null%</text>
<text id="77100410">Undead, eh? Well, you've come a long way.</text>
<text id="77100411">%null%</text>
<text id="77100412">%null%</text>
<text id="77100413">%null%</text>
<text id="77100414">%null%</text>
<text id="77100415">%null%</text>
<text id="77100416">%null%</text>
<text id="77100417">%null%</text>
<text id="77100418">%null%</text>
<text id="77100419">%null%</text>
<text id="77100420">Are you tormented by memories? Burdened by guilt?</text>
<text id="77100421">%null%</text>
<text id="77100422">%null%</text>
<text id="77100423">%null%</text>
<text id="77100424">%null%</text>
<text id="77100425">%null%</text>
<text id="77100426">%null%</text>
<text id="77100427">%null%</text>
<text id="77100428">%null%</text>
<text id="77100429">%null%</text>
<text id="77100430">Now the question. Are you ready for more?
If you are, then we ought to talk...</text>
<text id="77100500">This? This contains my power.</text>
<text id="77100501">%null%</text>
<text id="77100502">%null%</text>
<text id="77100503">%null%</text>
<text id="77100504">%null%</text>
<text id="77100505">%null%</text>
<text id="77100506">%null%</text>
<text id="77100507">%null%</text>
<text id="77100508">%null%</text>
<text id="77100509">%null%</text>
<text id="77100510">Long ago...this...naïve vessel of mine
set about devising new spells.</text>
<text id="77100511">%null%</text>
<text id="77100512">%null%</text>
<text id="77100513">%null%</text>
<text id="77100514">%null%</text>
<text id="77100515">%null%</text>
<text id="77100516">%null%</text>
<text id="77100517">%null%</text>
<text id="77100518">%null%</text>
<text id="77100519">%null%</text>
<text id="77100520">The fool dreamt of bringing new forms
of magic into the world.</text>
<text id="77100521">%null%</text>
<text id="77100522">%null%</text>
<text id="77100523">%null%</text>
<text id="77100524">%null%</text>
<text id="77100525">%null%</text>
<text id="77100526">%null%</text>
<text id="77100527">%null%</text>
<text id="77100528">%null%</text>
<text id="77100529">%null%</text>
<text id="77100530">But instead, he created me.
Mostly by chance, but he did a fine job, I must say.</text>
<text id="77100531">%null%</text>
<text id="77100532">%null%</text>
<text id="77100533">%null%</text>
<text id="77100534">%null%</text>
<text id="77100535">%null%</text>
<text id="77100536">%null%</text>
<text id="77100537">%null%</text>
<text id="77100538">%null%</text>
<text id="77100539">%null%</text>
<text id="77100540">What he sought was strength.
And so, I decided to demonstrate.</text>
<text id="77100541">%null%</text>
<text id="77100542">%null%</text>
<text id="77100543">%null%</text>
<text id="77100544">%null%</text>
<text id="77100545">%null%</text>
<text id="77100546">%null%</text>
<text id="77100547">%null%</text>
<text id="77100548">%null%</text>
<text id="77100549">%null%</text>
<text id="77100550">While my vessel slept,
I'd find my mark, and hunt him down.</text>
<text id="77100551">%null%</text>
<text id="77100552">%null%</text>
<text id="77100553">%null%</text>
<text id="77100554">%null%</text>
<text id="77100555">%null%</text>
<text id="77100556">%null%</text>
<text id="77100557">%null%</text>
<text id="77100558">%null%</text>
<text id="77100559">%null%</text>
<text id="77100560">But my vessel did not approve.
And so while I slept, he sealed the both of us away.</text>
<text id="77100800">Now, I don't have anything against humans,
but how is it that you go about defining good and evil?</text>
<text id="77100801">%null%</text>
<text id="77100802">%null%</text>
<text id="77100803">%null%</text>
<text id="77100804">%null%</text>
<text id="77100805">%null%</text>
<text id="77100806">%null%</text>
<text id="77100807">%null%</text>
<text id="77100808">%null%</text>
<text id="77100809">%null%</text>
<text id="77100810">I'm only using what the Gods gave me.
How can that be so wrong?</text>
<text id="77100830">So, what say you to a spot of murder on my behalf?</text>
<text id="77100831">%null%</text>
<text id="77100832">%null%</text>
<text id="77100833">%null%</text>
<text id="77100834">%null%</text>
<text id="77100835">%null%</text>
<text id="77100836">%null%</text>
<text id="77100837">%null%</text>
<text id="77100838">%null%</text>
<text id="77100839">%null%</text>
<text id="77100840">If you don't, I'll leave this place,
and take more lives. Many more.</text>
<text id="77100841">%null%</text>
<text id="77100842">%null%</text>
<text id="77100843">%null%</text>
<text id="77100844">%null%</text>
<text id="77100845">%null%</text>
<text id="77100846">%null%</text>
<text id="77100847">%null%</text>
<text id="77100848">%null%</text>
<text id="77100849">%null%</text>
<text id="77100850">I couldn't give a fig either way.
So, what's your choice?</text>
<text id="77100900">Yes, I see, how very kind of you.</text>
<text id="77100901">%null%</text>
<text id="77100902">%null%</text>
<text id="77100903">%null%</text>
<text id="77100904">%null%</text>
<text id="77100905">%null%</text>
<text id="77100906">%null%</text>
<text id="77100907">%null%</text>
<text id="77100908">%null%</text>
<text id="77100909">%null%</text>
<text id="77100910">I want you to kill a merchant named Gilligan.
A greedy, foul man. He won't be missed, I assure you.</text>
<text id="77100911">%null%</text>
<text id="77100912">%null%</text>
<text id="77100913">%null%</text>
<text id="77100914">%null%</text>
<text id="77100915">%null%</text>
<text id="77100916">%null%</text>
<text id="77100917">%null%</text>
<text id="77100918">%null%</text>
<text id="77100919">%null%</text>
<text id="77100920">Hmm...yes...
Bring me a Ladder Miniature as proof.</text>
<text id="77100921">%null%</text>
<text id="77100922">%null%</text>
<text id="77100923">%null%</text>
<text id="77100924">%null%</text>
<text id="77100925">%null%</text>
<text id="77100926">%null%</text>
<text id="77100927">%null%</text>
<text id="77100928">%null%</text>
<text id="77100929">%null%</text>
<text id="77100930">He's the only person who'd have
such an odd knick-knack... Heh heh...</text>
<text id="77101000">I want you to kill a merchant named Gilligan.</text>
<text id="77101001">%null%</text>
<text id="77101002">%null%</text>
<text id="77101003">%null%</text>
<text id="77101004">%null%</text>
<text id="77101005">%null%</text>
<text id="77101006">%null%</text>
<text id="77101007">%null%</text>
<text id="77101008">%null%</text>
<text id="77101009">%null%</text>
<text id="77101010">And don't forget that Ladder Miniature. Heh heh...</text>
<text id="77101100">Yes, yes, of course.
Forget that I asked.</text>
<text id="77101101">%null%</text>
<text id="77101102">%null%</text>
<text id="77101103">%null%</text>
<text id="77101104">%null%</text>
<text id="77101105">%null%</text>
<text id="77101106">%null%</text>
<text id="77101107">%null%</text>
<text id="77101108">%null%</text>
<text id="77101109">%null%</text>
<text id="77101110">That's what "good people" do, am I right?</text>
<text id="77101111">%null%</text>
<text id="77101112">%null%</text>
<text id="77101113">%null%</text>
<text id="77101114">%null%</text>
<text id="77101115">%null%</text>
<text id="77101116">%null%</text>
<text id="77101117">%null%</text>
<text id="77101118">%null%</text>
<text id="77101119">%null%</text>
<text id="77101120">Your feeble mind can't begin to comprehend me.
Not that you've ever tried! Heh heh...</text>
<text id="77101200">Heh heh heh... Fine work.</text>
<text id="77101201">%null%</text>
<text id="77101202">%null%</text>
<text id="77101203">%null%</text>
<text id="77101204">%null%</text>
<text id="77101205">%null%</text>
<text id="77101206">%null%</text>
<text id="77101207">%null%</text>
<text id="77101208">%null%</text>
<text id="77101209">%null%</text>
<text id="77101210">This is my thanks. Take it.</text>
<text id="77101211">%null%</text>
<text id="77101212">%null%</text>
<text id="77101213">%null%</text>
<text id="77101214">%null%</text>
<text id="77101215">%null%</text>
<text id="77101216">%null%</text>
<text id="77101217">%null%</text>
<text id="77101218">%null%</text>
<text id="77101219">%null%</text>
<text id="77101220">Now, let's discuss our next mark.</text>
<text id="77101221">%null%</text>
<text id="77101222">%null%</text>
<text id="77101223">%null%</text>
<text id="77101224">%null%</text>
<text id="77101225">%null%</text>
<text id="77101226">%null%</text>
<text id="77101227">%null%</text>
<text id="77101228">%null%</text>
<text id="77101229">%null%</text>
<text id="77101230">Now, let's discuss something of importance.</text>
<text id="77101300">I want you to kill one more.</text>
<text id="77101301">%null%</text>
<text id="77101302">%null%</text>
<text id="77101303">%null%</text>
<text id="77101304">%null%</text>
<text id="77101305">%null%</text>
<text id="77101306">%null%</text>
<text id="77101307">%null%</text>
<text id="77101308">%null%</text>
<text id="77101309">%null%</text>
<text id="77101310">Your target will be Cale,
the man who's drafting that map.</text>
<text id="77101311">%null%</text>
<text id="77101312">%null%</text>
<text id="77101313">%null%</text>
<text id="77101314">%null%</text>
<text id="77101315">%null%</text>
<text id="77101316">%null%</text>
<text id="77101317">%null%</text>
<text id="77101318">%null%</text>
<text id="77101319">%null%</text>
<text id="77101320">While I was trapped in here, he touched me,
that maniac, like I was some rare specimen.</text>
<text id="77101321">%null%</text>
<text id="77101322">%null%</text>
<text id="77101323">%null%</text>
<text id="77101324">%null%</text>
<text id="77101325">%null%</text>
<text id="77101326">%null%</text>
<text id="77101327">%null%</text>
<text id="77101328">%null%</text>
<text id="77101329">%null%</text>
<text id="77101330">He wore a strange helm.
Bring it back as proof.</text>
<text id="77101400">Yes, I knew I could count on you.</text>
<text id="77101401">%null%</text>
<text id="77101402">%null%</text>
<text id="77101403">%null%</text>
<text id="77101404">%null%</text>
<text id="77101405">%null%</text>
<text id="77101406">%null%</text>
<text id="77101407">%null%</text>
<text id="77101408">%null%</text>
<text id="77101409">%null%</text>
<text id="77101410">Perhaps this will slake my thirst. </text>
<text id="77101500">I want you to kill Cale,
the cartographer.</text>
<text id="77101501">%null%</text>
<text id="77101502">%null%</text>
<text id="77101503">%null%</text>
<text id="77101504">%null%</text>
<text id="77101505">%null%</text>
<text id="77101506">%null%</text>
<text id="77101507">%null%</text>
<text id="77101508">%null%</text>
<text id="77101509">%null%</text>
<text id="77101510">Bring his helm as proof.</text>
<text id="77101600">I want you to kill one more.</text>
<text id="77101601">%null%</text>
<text id="77101602">%null%</text>
<text id="77101603">%null%</text>
<text id="77101604">%null%</text>
<text id="77101605">%null%</text>
<text id="77101606">%null%</text>
<text id="77101607">%null%</text>
<text id="77101608">%null%</text>
<text id="77101609">%null%</text>
<text id="77101610">You target is Felkin, the hexer.</text>
<text id="77101611">%null%</text>
<text id="77101612">%null%</text>
<text id="77101613">%null%</text>
<text id="77101614">%null%</text>
<text id="77101615">%null%</text>
<text id="77101616">%null%</text>
<text id="77101617">%null%</text>
<text id="77101618">%null%</text>
<text id="77101619">%null%</text>
<text id="77101620">As proof, bring me that peculiar sceptre of his.</text>
<text id="77101621">%null%</text>
<text id="77101622">%null%</text>
<text id="77101623">%null%</text>
<text id="77101624">%null%</text>
<text id="77101625">%null%</text>
<text id="77101626">%null%</text>
<text id="77101627">%null%</text>
<text id="77101628">%null%</text>
<text id="77101629">%null%</text>
<text id="77101630">As proof, bring me a hex of his creation.</text>
<text id="77101631">%null%</text>
<text id="77101632">%null%</text>
<text id="77101633">%null%</text>
<text id="77101634">%null%</text>
<text id="77101635">%null%</text>
<text id="77101636">%null%</text>
<text id="77101637">%null%</text>
<text id="77101638">%null%</text>
<text id="77101639">%null%</text>
<text id="77101640">What do you say?</text>
<text id="77101700">Yes, very good.</text>
<text id="77101701">%null%</text>
<text id="77101702">%null%</text>
<text id="77101703">%null%</text>
<text id="77101704">%null%</text>
<text id="77101705">%null%</text>
<text id="77101706">%null%</text>
<text id="77101707">%null%</text>
<text id="77101708">%null%</text>
<text id="77101709">%null%</text>
<text id="77101710">But he will be formidable. Be cautious.</text>
<text id="77101800">I want you to kill Felkin.
That's Felkin, the hexer.</text>
<text id="77101801">%null%</text>
<text id="77101802">%null%</text>
<text id="77101803">%null%</text>
<text id="77101804">%null%</text>
<text id="77101805">%null%</text>
<text id="77101806">%null%</text>
<text id="77101807">%null%</text>
<text id="77101808">%null%</text>
<text id="77101809">%null%</text>
<text id="77101810">He has that sceptre. Bring it to me as proof.</text>
<text id="77101811">%null%</text>
<text id="77101812">%null%</text>
<text id="77101813">%null%</text>
<text id="77101814">%null%</text>
<text id="77101815">%null%</text>
<text id="77101816">%null%</text>
<text id="77101817">%null%</text>
<text id="77101818">%null%</text>
<text id="77101819">%null%</text>
<text id="77101820">Bring me a hex of his creation as proof.</text>
<text id="77102000">You could kill a dozen like that, and it wouldn't matter.
I want you to kill someone of import.</text>
<text id="77102001">%null%</text>
<text id="77102002">%null%</text>
<text id="77102003">%null%</text>
<text id="77102004">%null%</text>
<text id="77102005">%null%</text>
<text id="77102006">%null%</text>
<text id="77102007">%null%</text>
<text id="77102008">%null%</text>
<text id="77102009">%null%</text>
<text id="77102010">I want you to kill the girl in Majula
known as the Muse.</text>
<text id="77102011">%null%</text>
<text id="77102012">%null%</text>
<text id="77102013">%null%</text>
<text id="77102014">%null%</text>
<text id="77102015">%null%</text>
<text id="77102016">%null%</text>
<text id="77102017">%null%</text>
<text id="77102018">%null%</text>
<text id="77102019">%null%</text>
<text id="77102020">They say that she is the last Fire Keeper.
But they say a lot of things...</text>
<text id="77102021">%null%</text>
<text id="77102022">%null%</text>
<text id="77102023">%null%</text>
<text id="77102024">%null%</text>
<text id="77102025">%null%</text>
<text id="77102026">%null%</text>
<text id="77102027">%null%</text>
<text id="77102028">%null%</text>
<text id="77102029">%null%</text>
<text id="77102030">That she's a gentle shepherd,
lighting the way for you cursed fools. Nonsense.</text>
<text id="77102031">%null%</text>
<text id="77102032">%null%</text>
<text id="77102033">%null%</text>
<text id="77102034">%null%</text>
<text id="77102035">%null%</text>
<text id="77102036">%null%</text>
<text id="77102037">%null%</text>
<text id="77102038">%null%</text>
<text id="77102039">%null%</text>
<text id="77102040">I want you to kill the strange girl in Majula.</text>
<text id="77102041">%null%</text>
<text id="77102042">%null%</text>
<text id="77102043">%null%</text>
<text id="77102044">%null%</text>
<text id="77102045">%null%</text>
<text id="77102046">%null%</text>
<text id="77102047">%null%</text>
<text id="77102048">%null%</text>
<text id="77102049">%null%</text>
<text id="77102050">Once she is dead, I will have no regrets,
and I will be content to remain here.</text>
<text id="77102051">%null%</text>
<text id="77102052">%null%</text>
<text id="77102053">%null%</text>
<text id="77102054">%null%</text>
<text id="77102055">%null%</text>
<text id="77102056">%null%</text>
<text id="77102057">%null%</text>
<text id="77102058">%null%</text>
<text id="77102059">%null%</text>
<text id="77102060">The girl will have a peculiar feather.
Bring it to me.</text>
<text id="77102061">%null%</text>
<text id="77102062">%null%</text>
<text id="77102063">%null%</text>
<text id="77102064">%null%</text>
<text id="77102065">%null%</text>
<text id="77102066">%null%</text>
<text id="77102067">%null%</text>
<text id="77102068">%null%</text>
<text id="77102069">%null%</text>
<text id="77102070">Now, go along. Heh heh...</text>
<text id="77102100">Having second thoughts?</text>
<text id="77102101">%null%</text>
<text id="77102102">%null%</text>
<text id="77102103">%null%</text>
<text id="77102104">%null%</text>
<text id="77102105">%null%</text>
<text id="77102106">%null%</text>
<text id="77102107">%null%</text>
<text id="77102108">%null%</text>
<text id="77102109">%null%</text>
<text id="77102110">It's your choice.</text>
<text id="77102111">%null%</text>
<text id="77102112">%null%</text>
<text id="77102113">%null%</text>
<text id="77102114">%null%</text>
<text id="77102115">%null%</text>
<text id="77102116">%null%</text>
<text id="77102117">%null%</text>
<text id="77102118">%null%</text>
<text id="77102119">%null%</text>
<text id="77102120">Ah hah hah, now you're getting nervous, hmm?</text>
<text id="77102200">Your mark is the girl in Majula known as the muse.</text>
<text id="77102201">%null%</text>
<text id="77102202">%null%</text>
<text id="77102203">%null%</text>
<text id="77102204">%null%</text>
<text id="77102205">%null%</text>
<text id="77102206">%null%</text>
<text id="77102207">%null%</text>
<text id="77102208">%null%</text>
<text id="77102209">%null%</text>
<text id="77102210">Your mark is that strange girl in Majula.</text>
<text id="77102211">%null%</text>
<text id="77102212">%null%</text>
<text id="77102213">%null%</text>
<text id="77102214">%null%</text>
<text id="77102215">%null%</text>
<text id="77102216">%null%</text>
<text id="77102217">%null%</text>
<text id="77102218">%null%</text>
<text id="77102219">%null%</text>
<text id="77102220">And don't forget that feather. Heh heh heh...</text>
<text id="77102300">Yes, that's the feather.</text>
<text id="77102301">%null%</text>
<text id="77102302">%null%</text>
<text id="77102303">%null%</text>
<text id="77102304">%null%</text>
<text id="77102305">%null%</text>
<text id="77102306">%null%</text>
<text id="77102307">%null%</text>
<text id="77102308">%null%</text>
<text id="77102309">%null%</text>
<text id="77102310">Good, oh that is very good!</text>
<text id="77102311">%null%</text>
<text id="77102312">%null%</text>
<text id="77102313">%null%</text>
<text id="77102314">%null%</text>
<text id="77102315">%null%</text>
<text id="77102316">%null%</text>
<text id="77102317">%null%</text>
<text id="77102318">%null%</text>
<text id="77102319">%null%</text>
<text id="77102320">Your reward, take it. Heh heh heh...</text>
<text id="77102400">Now I am at peace.</text>
<text id="77102401">%null%</text>
<text id="77102402">%null%</text>
<text id="77102403">%null%</text>
<text id="77102404">%null%</text>
<text id="77102405">%null%</text>
<text id="77102406">%null%</text>
<text id="77102407">%null%</text>
<text id="77102408">%null%</text>
<text id="77102409">%null%</text>
<text id="77102410">I will sit here, and watch the world turn. Heh heh...</text>
<text id="77102500">What do you require?</text>
<text id="77102501">%null%</text>
<text id="77102502">%null%</text>
<text id="77102503">%null%</text>
<text id="77102504">%null%</text>
<text id="77102505">%null%</text>
<text id="77102506">%null%</text>
<text id="77102507">%null%</text>
<text id="77102508">%null%</text>
<text id="77102509">%null%</text>
<text id="77102510">You've been good to me. Go on, tell me what you need.</text>
<text id="77102600">Find what you wanted? Hah hah hah hah...</text>
<text id="77102700">Well, as you wish. Heh heh...</text>
<text id="77102800">Oh, no?</text>
<text id="77102900">Hmgh...</text>
<text id="77102901">%null%</text>
<text id="77102902">%null%</text>
<text id="77102903">%null%</text>
<text id="77102904">%null%</text>
<text id="77102905">%null%</text>
<text id="77102906">%null%</text>
<text id="77102907">%null%</text>
<text id="77102908">%null%</text>
<text id="77102909">%null%</text>
<text id="77102910">I hope you appreciate the ramifications.</text>
<text id="77102911">%null%</text>
<text id="77102912">%null%</text>
<text id="77102913">%null%</text>
<text id="77102914">%null%</text>
<text id="77102915">%null%</text>
<text id="77102916">%null%</text>
<text id="77102917">%null%</text>
<text id="77102918">%null%</text>
<text id="77102919">%null%</text>
<text id="77102920">Well, no matter. What's done is done.</text>
<text id="77102921">%null%</text>
<text id="77102922">%null%</text>
<text id="77102923">%null%</text>
<text id="77102924">%null%</text>
<text id="77102925">%null%</text>
<text id="77102926">%null%</text>
<text id="77102927">%null%</text>
<text id="77102928">%null%</text>
<text id="77102929">%null%</text>
<text id="77102930">Besides, I've found myself a new mark... Gah hah hah...</text>
<text id="77103000">I'll spend some time travelling the lands...</text>
<text id="77103001">%null%</text>
<text id="77103002">%null%</text>
<text id="77103003">%null%</text>
<text id="77103004">%null%</text>
<text id="77103005">%null%</text>
<text id="77103006">%null%</text>
<text id="77103007">%null%</text>
<text id="77103008">%null%</text>
<text id="77103009">%null%</text>
<text id="77103010">Slowly tracking my new mark...</text>
<text id="77103100">By the very gods... What have you done...</text>
<text id="77103101">%null%</text>
<text id="77103102">%null%</text>
<text id="77103103">%null%</text>
<text id="77103104">%null%</text>
<text id="77103105">%null%</text>
<text id="77103106">%null%</text>
<text id="77103107">%null%</text>
<text id="77103108">%null%</text>
<text id="77103109">%null%</text>
<text id="77103110">You've really done it... You'll never escape him...</text>
<text id="77105000">You are most intriguing. Heh heh...</text>
<text id="77105001">%null%</text>
<text id="77105002">%null%</text>
<text id="77105003">%null%</text>
<text id="77105004">%null%</text>
<text id="77105005">%null%</text>
<text id="77105006">%null%</text>
<text id="77105007">%null%</text>
<text id="77105008">%null%</text>
<text id="77105009">%null%</text>
<text id="77105010">Let's get right to it, shall we?</text>
<text id="77105011">%null%</text>
<text id="77105012">%null%</text>
<text id="77105013">%null%</text>
<text id="77105014">%null%</text>
<text id="77105015">%null%</text>
<text id="77105016">%null%</text>
<text id="77105017">%null%</text>
<text id="77105018">%null%</text>
<text id="77105019">%null%</text>
<text id="77105020">Hmm, ooh... Hah hah hah hah! Not bad at all.</text>
<text id="77105021">%null%</text>
<text id="77105022">%null%</text>
<text id="77105023">%null%</text>
<text id="77105024">%null%</text>
<text id="77105025">%null%</text>
<text id="77105026">%null%</text>
<text id="77105027">%null%</text>
<text id="77105028">%null%</text>
<text id="77105029">%null%</text>
<text id="77105030">Hmm...</text>
<text id="77105031">%null%</text>
<text id="77105032">%null%</text>
<text id="77105033">%null%</text>
<text id="77105034">%null%</text>
<text id="77105035">%null%</text>
<text id="77105036">%null%</text>
<text id="77105037">%null%</text>
<text id="77105038">%null%</text>
