<entries>
<text id="70500000">Heh heh heh... What seems to be the ruckus?</text>
<text id="70500001">%null%</text>
<text id="70500002">%null%</text>
<text id="70500003">%null%</text>
<text id="70500004">%null%</text>
<text id="70500005">%null%</text>
<text id="70500006">%null%</text>
<text id="70500007">%null%</text>
<text id="70500008">%null%</text>
<text id="70500009">%null%</text>
<text id="70500010">Ooh, my! Your face.</text>
<text id="70500011">%null%</text>
<text id="70500012">%null%</text>
<text id="70500013">%null%</text>
<text id="70500014">%null%</text>
<text id="70500015">%null%</text>
<text id="70500016">%null%</text>
<text id="70500017">%null%</text>
<text id="70500018">%null%</text>
<text id="70500019">%null%</text>
<text id="70500020">The face of the curse.</text>
<text id="70500021">%null%</text>
<text id="70500022">%null%</text>
<text id="70500023">%null%</text>
<text id="70500024">%null%</text>
<text id="70500025">%null%</text>
<text id="70500026">%null%</text>
<text id="70500027">%null%</text>
<text id="70500028">%null%</text>
<text id="70500029">%null%</text>
<text id="70500030">It's an Undead.</text>
<text id="70500031">%null%</text>
<text id="70500032">%null%</text>
<text id="70500033">%null%</text>
<text id="70500034">%null%</text>
<text id="70500035">%null%</text>
<text id="70500036">%null%</text>
<text id="70500037">%null%</text>
<text id="70500038">%null%</text>
<text id="70500039">%null%</text>
<text id="70500040">An Undead has come to play. Heh heh...</text>
<text id="70500041">%null%</text>
<text id="70500042">%null%</text>
<text id="70500043">%null%</text>
<text id="70500044">%null%</text>
<text id="70500045">%null%</text>
<text id="70500046">%null%</text>
<text id="70500047">%null%</text>
<text id="70500048">%null%</text>
<text id="70500049">%null%</text>
<text id="70500050">You've seen the mark upon your flesh.</text>
<text id="70500051">%null%</text>
<text id="70500052">%null%</text>
<text id="70500053">%null%</text>
<text id="70500054">%null%</text>
<text id="70500055">%null%</text>
<text id="70500056">%null%</text>
<text id="70500057">%null%</text>
<text id="70500058">%null%</text>
<text id="70500059">%null%</text>
<text id="70500060">The sign of the Undead...symbol of the accursed.</text>
<text id="70500061">%null%</text>
<text id="70500062">%null%</text>
<text id="70500063">%null%</text>
<text id="70500064">%null%</text>
<text id="70500065">%null%</text>
<text id="70500066">%null%</text>
<text id="70500067">%null%</text>
<text id="70500068">%null%</text>
<text id="70500069">%null%</text>
<text id="70500070">They all end up here, all the ones like you.</text>
<text id="70500071">%null%</text>
<text id="70500072">%null%</text>
<text id="70500073">%null%</text>
<text id="70500074">%null%</text>
<text id="70500075">%null%</text>
<text id="70500076">%null%</text>
<text id="70500077">%null%</text>
<text id="70500078">%null%</text>
<text id="70500079">%null%</text>
<text id="70500080">You spoke to that kind old dear, didn't you?  Heh heh...</text>
<text id="70500081">%null%</text>
<text id="70500082">%null%</text>
<text id="70500083">%null%</text>
<text id="70500084">%null%</text>
<text id="70500085">%null%</text>
<text id="70500086">%null%</text>
<text id="70500087">%null%</text>
<text id="70500088">%null%</text>
<text id="70500089">%null%</text>
<text id="70500090">%null%</text>
<text id="70500091">%null%</text>
<text id="70500092">%null%</text>
<text id="70500093">%null%</text>
<text id="70500094">%null%</text>
<text id="70500095">%null%</text>
<text id="70500096">%null%</text>
<text id="70500097">%null%</text>
<text id="70500098">%null%</text>
<text id="70500099">%null%</text>
<text id="70500100">You're finished.</text>
<text id="70500101">%null%</text>
<text id="70500102">%null%</text>
<text id="70500103">%null%</text>
<text id="70500104">%null%</text>
<text id="70500105">%null%</text>
<text id="70500106">%null%</text>
<text id="70500107">%null%</text>
<text id="70500108">%null%</text>
<text id="70500109">%null%</text>
<text id="70500110">Your flesh will decay, your mind will fade.
But you won't ever die.</text>
<text id="70500111">%null%</text>
<text id="70500112">%null%</text>
<text id="70500113">%null%</text>
<text id="70500114">%null%</text>
<text id="70500115">%null%</text>
<text id="70500116">%null%</text>
<text id="70500117">%null%</text>
<text id="70500118">%null%</text>
<text id="70500119">%null%</text>
<text id="70500120">You'll go Hollow.</text>
<text id="70500121">%null%</text>
<text id="70500122">%null%</text>
<text id="70500123">%null%</text>
<text id="70500124">%null%</text>
<text id="70500125">%null%</text>
<text id="70500126">%null%</text>
<text id="70500127">%null%</text>
<text id="70500128">%null%</text>
<text id="70500129">%null%</text>
<text id="70500130">Yes, you will become one of them.</text>
<text id="70500131">%null%</text>
<text id="70500132">%null%</text>
<text id="70500133">%null%</text>
<text id="70500134">%null%</text>
<text id="70500135">%null%</text>
<text id="70500136">%null%</text>
<text id="70500137">%null%</text>
<text id="70500138">%null%</text>
<text id="70500139">%null%</text>
<text id="70500140">Hollows prey upon men, feast upon their souls.</text>
<text id="70500141">%null%</text>
<text id="70500142">%null%</text>
<text id="70500143">%null%</text>
<text id="70500144">%null%</text>
<text id="70500145">%null%</text>
<text id="70500146">%null%</text>
<text id="70500147">%null%</text>
<text id="70500148">%null%</text>
<text id="70500149">%null%</text>
<text id="70500150">This is the fate of the cursed. Heh heh heh...</text>
<text id="70500151">%null%</text>
<text id="70500152">%null%</text>
<text id="70500153">%null%</text>
<text id="70500154">%null%</text>
<text id="70500155">%null%</text>
<text id="70500156">%null%</text>
<text id="70500157">%null%</text>
<text id="70500158">%null%</text>
<text id="70500159">%null%</text>
<text id="70500160">%null%</text>
<text id="70500161">%null%</text>
<text id="70500162">%null%</text>
<text id="70500163">%null%</text>
<text id="70500164">%null%</text>
<text id="70500165">%null%</text>
<text id="70500166">%null%</text>
<text id="70500167">%null%</text>
<text id="70500168">%null%</text>
<text id="70500169">%null%</text>
<text id="70500170">What is your name?</text>
<text id="70500171">%null%</text>
<text id="70500172">%null%</text>
<text id="70500173">%null%</text>
<text id="70500174">%null%</text>
<text id="70500175">%null%</text>
<text id="70500176">%null%</text>
<text id="70500177">%null%</text>
<text id="70500178">%null%</text>
<text id="70500179">%null%</text>
<text id="70500180">Surely you can tell me your name.</text>
<text id="70500181">%null%</text>
<text id="70500182">%null%</text>
<text id="70500183">%null%</text>
<text id="70500184">%null%</text>
<text id="70500185">%null%</text>
<text id="70500186">%null%</text>
<text id="70500187">%null%</text>
<text id="70500188">%null%</text>
<text id="70500189">%null%</text>
<text id="70500190">If you remember it, that is. Heh heh...</text>
<text id="70500191">%null%</text>
<text id="70500192">%null%</text>
<text id="70500193">%null%</text>
<text id="70500194">%null%</text>
<text id="70500195">%null%</text>
<text id="70500196">%null%</text>
<text id="70500197">%null%</text>
<text id="70500198">%null%</text>
<text id="70500199">%null%</text>
<text id="70500200">At least you know your own name.</text>
<text id="70500201">%null%</text>
<text id="70500202">%null%</text>
<text id="70500203">%null%</text>
<text id="70500204">%null%</text>
<text id="70500205">%null%</text>
<text id="70500206">%null%</text>
<text id="70500207">%null%</text>
<text id="70500208">%null%</text>
<text id="70500209">%null%</text>
<text id="70500210">Here's your reward for sharing.</text>
<text id="70500211">%null%</text>
<text id="70500212">%null%</text>
<text id="70500213">%null%</text>
<text id="70500214">%null%</text>
<text id="70500215">%null%</text>
<text id="70500216">%null%</text>
<text id="70500217">%null%</text>
<text id="70500218">%null%</text>
<text id="70500219">%null%</text>
<text id="70500220">It's a Human Effigy.</text>
<text id="70500221">%null%</text>
<text id="70500222">%null%</text>
<text id="70500223">%null%</text>
<text id="70500224">%null%</text>
<text id="70500225">%null%</text>
<text id="70500226">%null%</text>
<text id="70500227">%null%</text>
<text id="70500228">%null%</text>
<text id="70500229">%null%</text>
<text id="70500230">Take a closer look...</text>
<text id="70500231">%null%</text>
<text id="70500232">%null%</text>
<text id="70500233">%null%</text>
<text id="70500234">%null%</text>
<text id="70500235">%null%</text>
<text id="70500236">%null%</text>
<text id="70500237">%null%</text>
<text id="70500238">%null%</text>
<text id="70500239">%null%</text>
<text id="70500240">Who do you think it's supposed to be?</text>
<text id="70500241">%null%</text>
<text id="70500242">%null%</text>
<text id="70500243">%null%</text>
<text id="70500244">%null%</text>
<text id="70500245">%null%</text>
<text id="70500246">%null%</text>
<text id="70500247">%null%</text>
<text id="70500248">%null%</text>
<text id="70500249">%null%</text>
<text id="70500250">It's somebody that you know very well.</text>
<text id="70500251">%null%</text>
<text id="70500252">%null%</text>
<text id="70500253">%null%</text>
<text id="70500254">%null%</text>
<text id="70500255">%null%</text>
<text id="70500256">%null%</text>
<text id="70500257">%null%</text>
<text id="70500258">%null%</text>
<text id="70500259">%null%</text>
<text id="70500260">Go on, look closer, closer still.</text>
<text id="70500261">%null%</text>
<text id="70500262">%null%</text>
<text id="70500263">%null%</text>
<text id="70500264">%null%</text>
<text id="70500265">%null%</text>
<text id="70500266">%null%</text>
<text id="70500267">%null%</text>
<text id="70500268">%null%</text>
<text id="70500269">%null%</text>
<text id="70500270">Think back, deep into your past.</text>
<text id="70500271">%null%</text>
<text id="70500272">%null%</text>
<text id="70500273">%null%</text>
<text id="70500274">%null%</text>
<text id="70500275">%null%</text>
<text id="70500276">%null%</text>
<text id="70500277">%null%</text>
<text id="70500278">%null%</text>
<text id="70500279">%null%</text>
<text id="70500280">Yes, it's an effigy of you.</text>
<text id="70500281">%null%</text>
<text id="70500282">%null%</text>
<text id="70500283">%null%</text>
<text id="70500284">%null%</text>
<text id="70500285">%null%</text>
<text id="70500286">%null%</text>
<text id="70500287">%null%</text>
<text id="70500288">%null%</text>
<text id="70500289">%null%</text>
<text id="70500290">All people come here for the same reason.</text>
<text id="70500291">%null%</text>
<text id="70500292">%null%</text>
<text id="70500293">%null%</text>
<text id="70500294">%null%</text>
<text id="70500295">%null%</text>
<text id="70500296">%null%</text>
<text id="70500297">%null%</text>
<text id="70500298">%null%</text>
<text id="70500299">%null%</text>
<text id="70500300">To break the curse.</text>
<text id="70500301">%null%</text>
<text id="70500302">%null%</text>
<text id="70500303">%null%</text>
<text id="70500304">%null%</text>
<text id="70500305">%null%</text>
<text id="70500306">%null%</text>
<text id="70500307">%null%</text>
<text id="70500308">%null%</text>
<text id="70500309">%null%</text>
<text id="70500310">You're no different, I should think?</text>
<text id="70500311">%null%</text>
<text id="70500312">%null%</text>
<text id="70500313">%null%</text>
<text id="70500314">%null%</text>
<text id="70500315">%null%</text>
<text id="70500316">%null%</text>
<text id="70500317">%null%</text>
<text id="70500318">%null%</text>
<text id="70500319">%null%</text>
<text id="70500320">Hmm... doesn't stand a chance.</text>
<text id="70500321">%null%</text>
<text id="70500322">%null%</text>
<text id="70500323">%null%</text>
<text id="70500324">%null%</text>
<text id="70500325">%null%</text>
<text id="70500326">%null%</text>
<text id="70500327">%null%</text>
<text id="70500328">%null%</text>
<text id="70500329">%null%</text>
<text id="70500330">Well, you never know!</text>
<text id="70500331">%null%</text>
<text id="70500332">%null%</text>
<text id="70500333">%null%</text>
<text id="70500334">%null%</text>
<text id="70500335">%null%</text>
<text id="70500336">%null%</text>
<text id="70500337">%null%</text>
<text id="70500338">%null%</text>
<text id="70500339">%null%</text>
<text id="70500340">%null%</text>
<text id="70500341">%null%</text>
<text id="70500342">%null%</text>
<text id="70500343">%null%</text>
<text id="70500344">%null%</text>
<text id="70500345">%null%</text>
<text id="70500346">%null%</text>
<text id="70500347">%null%</text>
<text id="70500348">%null%</text>
<text id="70500349">%null%</text>
<text id="70500350">Now, it's just about our bedtime.</text>
<text id="70500351">%null%</text>
<text id="70500352">%null%</text>
<text id="70500353">%null%</text>
<text id="70500354">%null%</text>
<text id="70500355">%null%</text>
<text id="70500356">%null%</text>
<text id="70500357">%null%</text>
<text id="70500358">%null%</text>
<text id="70500359">%null%</text>
<text id="70500360">Go through the door and trot along to the kingdom.
</text>
<text id="70500361">%null%</text>
<text id="70500362">%null%</text>
<text id="70500363">%null%</text>
<text id="70500364">%null%</text>
<text id="70500365">%null%</text>
<text id="70500366">%null%</text>
<text id="70500367">%null%</text>
<text id="70500368">%null%</text>
<text id="70500369">%null%</text>
<text id="70500370">Go on, out the door.</text>
<text id="70500371">%null%</text>
<text id="70500372">%null%</text>
<text id="70500373">%null%</text>
<text id="70500374">%null%</text>
<text id="70500375">%null%</text>
<text id="70500376">%null%</text>
<text id="70500377">%null%</text>
<text id="70500378">%null%</text>
<text id="70500379">%null%</text>
<text id="70500380">But remember, hold on to your souls.</text>
<text id="70500381">%null%</text>
<text id="70500382">%null%</text>
<text id="70500383">%null%</text>
<text id="70500384">%null%</text>
<text id="70500385">%null%</text>
<text id="70500386">%null%</text>
<text id="70500387">%null%</text>
<text id="70500388">%null%</text>
<text id="70500389">%null%</text>
<text id="70500390">They're all that keep you from going Hollow.</text>
<text id="70500391">%null%</text>
<text id="70500392">%null%</text>
<text id="70500393">%null%</text>
<text id="70500394">%null%</text>
<text id="70500395">%null%</text>
<text id="70500396">%null%</text>
<text id="70500397">%null%</text>
<text id="70500398">%null%</text>
<text id="70500399">%null%</text>
<text id="70500400">Oh, I'll fool you no longer...</text>
<text id="70500401">%null%</text>
<text id="70500402">%null%</text>
<text id="70500403">%null%</text>
<text id="70500404">%null%</text>
<text id="70500405">%null%</text>
<text id="70500406">%null%</text>
<text id="70500407">%null%</text>
<text id="70500408">%null%</text>
<text id="70500409">%null%</text>
<text id="70500410">You'll lose your souls... All of them. Over and over again.</text>
<text id="70500411">%null%</text>
<text id="70500412">%null%</text>
<text id="70500413">%null%</text>
<text id="70500414">%null%</text>
<text id="70500415">%null%</text>
<text id="70500416">%null%</text>
<text id="70500417">%null%</text>
<text id="70500418">%null%</text>
<text id="70500419">%null%</text>
<text id="70500420">%null%</text>
<text id="70501000">You must go, on a journey without rest.</text>
<text id="70501001">%null%</text>
<text id="70501002">%null%</text>
<text id="70501003">%null%</text>
<text id="70501004">%null%</text>
<text id="70501005">%null%</text>
<text id="70501006">%null%</text>
<text id="70501007">%null%</text>
<text id="70501008">%null%</text>
<text id="70501009">%null%</text>
<text id="70501010">Well...I suppose, if you find yourself at an impasse...</text>
<text id="70501011">%null%</text>
<text id="70501012">%null%</text>
<text id="70501013">%null%</text>
<text id="70501014">%null%</text>
<text id="70501015">%null%</text>
<text id="70501016">%null%</text>
<text id="70501017">%null%</text>
<text id="70501018">%null%</text>
<text id="70501019">%null%</text>
<text id="70501020">But if your will is yet unbroken,
then you may return here.</text>
<text id="70501021">%null%</text>
<text id="70501022">%null%</text>
<text id="70501023">%null%</text>
<text id="70501024">%null%</text>
<text id="70501025">%null%</text>
<text id="70501026">%null%</text>
<text id="70501027">%null%</text>
<text id="70501028">%null%</text>
<text id="70501029">%null%</text>
<text id="70501030">To start again, with a clean slate...</text>
<text id="70501031">%null%</text>
<text id="70501032">%null%</text>
<text id="70501033">%null%</text>
<text id="70501034">%null%</text>
<text id="70501035">%null%</text>
<text id="70501036">%null%</text>
<text id="70501037">%null%</text>
<text id="70501038">%null%</text>
<text id="70501039">%null%</text>
<text id="70501040">Hah hah hah...</text>
<text id="70501100">Now, go along, go along... Heh heh...</text>
<text id="70501200">Still determined as ever, I see... Heh heh...</text>
<text id="70501201">%null%</text>
<text id="70501202">%null%</text>
<text id="70501203">%null%</text>
<text id="70501204">%null%</text>
<text id="70501205">%null%</text>
<text id="70501206">%null%</text>
<text id="70501207">%null%</text>
<text id="70501208">%null%</text>
<text id="70501209">%null%</text>
<text id="70501210">Do you wish to start all over again?</text>
<text id="70501300">No, of course not.</text>
<text id="70501301">%null%</text>
<text id="70501302">%null%</text>
<text id="70501303">%null%</text>
<text id="70501304">%null%</text>
<text id="70501305">%null%</text>
<text id="70501306">%null%</text>
<text id="70501307">%null%</text>
<text id="70501308">%null%</text>
<text id="70501309">%null%</text>
<text id="70501310">Now, go along then... Heh heh...</text>
<text id="70501400">Oh, but you don't have what's needed.</text>
