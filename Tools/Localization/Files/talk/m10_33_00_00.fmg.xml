<text id="75601737">%null%</text>
<text id="75601738">%null%</text>
<text id="75601739">%null%</text>
<text id="75601740">And that is why I pity the fools called humans.</text>
<text id="75601741">%null%</text>
<text id="75601742">%null%</text>
<text id="75601743">%null%</text>
<text id="75601744">%null%</text>
